<body class="nav-md">
<div class="container body">
    <div class="main_container">
        <include file="Common:menu"/>
        <!-- page content -->
        <div class="right_col" role="main">
            <div class="row">
                <div class="col-md-12 col-sm-12 col-xs-12">
                    <div class="x_panel">
                        <div class="x_title">
                            <h2>扫码记录</h2>
                            <div class="clearfix"></div>
                        </div>
                        <div class="x_content">
                            <div class="table-responsive">
                                <table id="datatable" class="table table-striped jambo_table table-bordered">
                                    <thead>
                                    <tr class="headings">
                                        <th class="column-title">用户名</th>
                                        <th class="column-title">场景id</th>
                                        <th class="column-title">扫码时间</th>
                                        <th class="column-title">助力人数</th>
                                    </tr>
                                    </thead>
                                    <tbody>
                                    <foreach name="sceneList" item="val">
                                        <tr class="even pointer">
                                            <td>{$val['username']}</td>
                                            <td>{$val['scene_id']}</td>
                                            <td>{$val['create_time'] | date="Y-m-d H:i:s",###}</td>
                                            <if condition="$val['count'] eq 0">
                                                <td>{$val['count']}</td>
                                                <else/>
                                                <td>{$val['count']}<a class="btn btn-success btn-xs btn_access" access_id="285" style="float: right;" href="{:U('Qrcode/helpList',array('user_id'=>$val['user_id'],'scene_id'=>$val['scene_id']))}">查看</a></td>
                                            </if>
                                        </tr>
                                    </foreach>
                                    </tbody>
                                </table>
                                <!--<div class="pagination pagination-small">{$page}</div>-->
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- /page content -->
        <!-- footer content -->
        <include file="Common:footer"/>
        <!-- /footer content -->
    </div>
</div>
<!-- bootstrap-progressbar -->
<script src="__PUBLIC__/vendors/bootstrap-progressbar/bootstrap-progressbar.min.js"></script>
