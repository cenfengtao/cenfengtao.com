            <div class="bargain">
                <section class="rate">
                    <div class="title">
                        <span></span>
                        <h3>砍价进度</h3>
                        <span></span>
                    </div>
                    <div class="light">
                        <div class="green">800元</div>
                        <div class="gray"><span>50元</span></div>
                    </div>
                </section>
                <section class="alert">
                    <div class="text">
                        <img src="../../../../Public/images/clock.png" alt="">
