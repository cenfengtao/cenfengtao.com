/*头部 start*/
.header{
    height: 1.2rem;
    line-height: 1.22rem;
    position: fixed;
    width: 100%;
    background: white;
    overflow: hidden;
    z-index: 100;
    clear: both;
}
.header a:first-child{
    left: 0.27rem;
    position: absolute;
}
.header a:first-child img{
    margin-top: 0.4rem;
    height: 0.5rem;
}
.header h3{
    text-align: center;
    font-size: 0.52rem;
    color: #065b9c;
    margin-left: 0.3rem;
    margin-top: 0.05rem;
}
/*头部 end*/
/*中部 start*/
.middle{
    padding-top: 1.4rem;
    background: #f0f1f1;
    padding-bottom: 1.5rem;
}
/*机构部分*/
.org{
	height: 3.8rem;
    margin-bottom: 0.2rem;
    background: white;
}
.org .lefts{
	width: 45%;
	float: left;
	margin-left: 5%;
	box-sizing:content-box;
}
.lefts img{
	height: 1.3rem;
	width: auto;
	margin-left: 5%;
	position: absolute;
	top: 0;
	bottom: 0;
	left: 0;
	right: 0;
	margin: auto;
}
.lefts .logo{
    height: 2rem;
    width: 100%;
    display: inline-block;
    position: relative;
}
.lefts>a{
    height: 0.7rem;
    width: 2.3rem;
    display: inline-block;
    font-size: 0.405rem;
    border-radius: 0.1rem;
    background: #386998;
    color: white;
    text-align: center;
    line-height: 0.7rem;
}
.lefts .seen{
    height: 0.7rem;
    width: 2.4rem;
    display: inline-block;
    font-size: 0.405rem;
    border-radius: 0.1rem;
    background: #db6066;
    color: white;
    text-align: center;
    line-height: 0.7rem;
    margin-left: 0.15rem;
}
.lefts p{
	text-align: center;
    font-size: 0.36rem;
    margin-top: 0.2rem;
}
.org .rights{
	width: 50%;
	float: right;
}
.rights ul{
	margin-top: 0.5rem;
}
.rights li{
	height: 0.9rem;
	line-height: 0.9rem;
	margin-left: 0.5rem;
    float: left;
}
.rights li span{
	font-size: 0.3647rem;
	float: left;
}
.rights li span:nth-child(2){
	margin-top: 0.1rem;
	margin-left: 0.2rem;
}
.rights li img{
	width: 0.52rem;
	height: 0.5rem;
	display: inline-block;;
}

.lunbo{
    height: 4.83rem;
    background: #386998;
    width: 100%;
    padding-top: 0.6rem;
}
.lunbo li{
    height: 4.2rem;
    width: 100%;
}
.lunbo li img{
    height: 4.2rem;
    width: 80%;
    margin: 0rem auto;
}
.banner {
        height: 4.2rem;
}

.banner .bd {
    height: 4.2rem;
    overflow: hidden;
}

.banner .bd li {
    height: 4.2rem;
    width: 100%;
}
.arrow-left{
    background: url(../../../../Public/images/arrows.png) no-repeat left top;
    position: absolute;
    left: 0.2rem;
    top: 50%;
    margin-top: -0.7rem;
    width: 1rem;
    height: 1.3rem;
    z-index: 10;
    background-size: 0.76rem;
}
.arrow-right{
    background: url(../../../../Public/images/arrows.png) no-repeat left bottom;
    position: absolute;
    right: 0.2rem;
    top: 50%;
    margin-top: -0.7rem;
    width: 1rem;
    height: 1.3rem;
    z-index: 10;
    background-size: 0.76rem;
}





.couponButton{
    text-align: center;
    padding: 0.3rem 0;
}
.couponButton span{
    display: inline-block;
    width: 90%;
    height: 1.15rem;
    line-height: 1.15rem;
    text-align: center;
    color: white;
    background: #db6066;
    font-size: 0.55rem;
    border-radius: 0.1rem;
}


/*精品课程*/
.course{
    background-color: white;
    overflow: hidden;
    /* padding-bottom: 0.45rem; */
    padding-bottom: 0.22rem;
    margin-bottom: 0.2rem;
}
.course >h3{
    font-size: 0.2669rem;
    color: #959696;
    font-weight: normal;
    text-align: center;
    height: 1rem;
    line-height: 1rem;
}
.course >h3 strong{
    font-size: 0.5rem;
    color: #386998;
    font-weight: normal;
}
.course .advert img{
    width: 100%;
    height: 1.62rem;
}
.courseContent{
    padding-top: 0.45rem;
}
.courseContent li{
    overflow: hidden;
    margin-bottom: 0.2rem;
    background: white;
    padding: 0.4rem 0;
}
.courseContent .img{
    position: relative;
    float: right;
    margin-right: 0.37rem;
}
.courseContent .img .courseImg{
    width: 5.39rem;
    height: 3.83rem;
}
.courseContent .tap{
    height: 1.04rem;
    width: 1.15rem;
    position: absolute;
    top: -0.05rem;
    left: 0.1rem;
}
.courseContent .logoTap{
    height: 1.04rem;
    width: 1.15rem;
    position: absolute;
}
.courseContent .img .imgLogo{
    width: 0.7rem;
    height: 0.7rem;
    position: absolute;
    top: 0.15rem;
    margin: 0 auto;
    left: 0;
    right: 0;
}
.courseContent .type{
    position: absolute;
    bottom: 0;
    left: 0;
    width: 100%;
    background: rgba(0,0,0,0.5);
    height: 0.8rem;
    line-height: 0.8rem;
    overflow: hidden;
}
.courseContent .type span{
    height: 0.56rem;
    line-height: 0.56rem;
    font-size: 0.34rem;
    padding: 0 0.15rem;
    color: white;
    display: inline-block;
    margin-right: 0.15rem;
    border-radius: 0.1rem;
}
.courseContent .type span:first-child{
    background-color: #ffc62d;
    margin-left: 0.3rem;
}
.courseContent .type span:nth-child(2){
    background-color: #e0656b;
}
.courseContent .type span:nth-child(3){
    background-color: #62b7e3;
}
.courseContent .text{
    float: left;
    margin-left: 0.24rem;
}
.courseContent .text h3{
    font-size: 0.45rem;
    line-height: 0.6rem;
    color: #386998;
    margin: 0.18rem 0rem 0.35rem;
    width: 5rem;
    height: 0.9rem;
    font-weight: normal;
}
.courseContent .text .groupPrice{
    font-size: 0.35rem;
    color: #386998;
    /*font-weight: bold;*/
}
.courseContent .text .moneySign{
    font-size: 0.4rem;
    color: #db6066;
}
.courseContent .groupPrice strong{
    font-size: 0.6rem;
    color: #db6066;
}
.courseContent .originalPrice{
    font-size: 0.3rem;
    color: #386998;
    text-decoration: line-through;
    margin-top: 0.1rem;
}
.courseContent .text .try{
    margin-top: 0.3rem;
}
.courseContent .text .try a{
    width: 2.15rem;
    height: 0.72rem;
    border-radius: 0.1rem;
    font-weight: bold;
    text-align: center;
    line-height: 0.72rem;
    font-size: 0.4rem;
    display: inline-block;
    margin-right: 0.3rem;
}
.courseContent .text .try a:first-child{
    border:0.05rem solid #83cf95;
    color: #83cf95;
}
.courseContent .text .try a:nth-child(2){
    border:0.05rem solid #db6066;
    color: #db6066;
}


