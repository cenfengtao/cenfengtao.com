.courseList .originalPrice{
    font-size: 0.36rem;
    color: #386998;
    text-decoration: line-through;
}
.courseList .text a{
    width: 5.1rem;
    height: 0.72rem;
    border-radius: 0.1rem;
    font-weight: bold;
    text-align: center;
    line-height: 0.72rem;
    font-size: 0.4rem;
    display: inline-block;
    margin-right: 0.25rem;
    border:0.05rem solid #db6066;
    color: #db6066;
    float: right;
    margin-top: 0.05rem;
    margin-bottom: 0.1rem;
}
.courseList .text .groupNum{
    clear: both;
}
.courseList .groupNum .finished{
    font-size: 0.2876rem;
    float: left;
    margin-top: 0.05rem
}
.courseList .groupNum .bar{
    width: 1.9rem;
    height: 0.2rem;
    border-radius: 0.2rem;
    float: left;
    margin-top: 0.1rem;
    border:0.04rem solid #db6066;
    margin-left: 0.1rem;
}
.courseList .groupNum .bar span{
    height: 0.2rem;
    width: 1rem;
    background-color: #ffa3a7;
    border-radius: 0.2rem;
    display: block;
    margin-left: -0.01rem;
    float: left;
}
.nowPeople,.allPeople{
    font-size: 0.2876rem;
}


/*商品团购*/
.shopGroups{
    /*margin-top: 0.2rem;*/
    overflow: hidden;
}
.shopGroups ul li{
    width: 5.3rem;
    height: 7.64rem;
    padding-left: 0.2rem;
    float: left;
    background: white;
    padding-top:0.35rem;
    margin-bottom: 0.2rem;
}
.shopGroups ul li:nth-child(odd){
    margin-right: 0.2rem;
}
.shopGroups .img{
    width: 5.11rem;
    height: 5.11rem;
    position: relative;
}
.shopGroups .img img{
    width: 100%;
    height: 100%;
    margin: 0 auto;
}
.shopGroups .img .time{
    width: 100%;
    height: 0.58rem;
    background: rgba(0,0,0,0.5);
    line-height: 0.58rem;
    position: absolute;
    top: 0;
    left: 0;
}
.shopGroups .img .time span{
    color: white;
    font-size: 0.3rem;
    margin-left: 0.24rem;
}
.shopGroups .img .type{
    width: 100%;
    height: 0.8rem;
    position: absolute;
    bottom: 0;
    left: 0;
    line-height: 0.8rem;
    background: rgba(0,0,0,0.5);
    overflow: hidden;
}
.shopGroups .type .originalPrice{
    font-size: 0.3rem;
    color: white;
    margin-left: 0.26rem;
    margin-right: 0.26rem;
}
.shopGroups .type .tab{
    font-size: 0.3401rem;
    color: white;
    padding: 0 0.25rem;
    margin-right: 0.1rem;
    border-radius: 0.1rem;
    height: 0.52rem;
    line-height: 0.52rem;
    display: inline-block;
}
.shopGroups .type span:nth-child(2){
    background: #6fbee4;
}
.shopGroups .type span:nth-child(3){
    background: #de8cbd;
}
.shopGroups .text h3{
    font-size: 0.48rem;
    margin-top: 0.1rem;
    color: #386998;
    font-weight: normal;
}
.shopGroups .text .groupPrice{
    font-size: 0.4rem;
    color: #386998;
}
.shopGroups .text .groupPrice strong{
    font-size: 0.6rem;
    color: #db6066;
}
.shopGroups .text div >a{
    font-size: 0.4rem;
    color: #83cf95;
    height: 0.72rem;
    width: 2rem;
    text-align: center;
    line-height: 0.72rem;
    border:0.05rem solid #83cf95;
    display: inline-block;
    border-radius: 0.1rem;
    float: right;
    margin-right: 0.15rem;
}
.shopGroups .text .groupNum{
    clear: both;
    overflow: hidden;
    margin-top: 0.2rem;
}
.shopGroups .groupNum .finished{
    font-size: 0.2876rem;
    float: left;
    margin-top: 0.05rem
}
.shopGroups .groupNum .bar{
    width: 1.9rem;
    height: 0.2rem;
    border-radius: 0.2rem;
    float: left;
    margin-top: 0.1rem;
    border:0.04rem solid #db6066;
    margin-left: 0.1rem;
}
.shopGroups .groupNum .bar span{
    height: 0.2rem;
    width: 1rem;
    background-color: #ffa3a7;
    border-radius: 0.2rem;
    display: block;
    margin-left: -0.01rem;
    float: left;
}
.nowPeople,.allPeople{
    font-size: 0.2876rem;
}


/*商品砍价*/
.shopBargain{
    overflow: hidden;
}
.shopBargain ul li{
    width: 5.3rem;
    height: 7.64rem;
    padding-left: 0.2rem;
    float: left;
    background: white;
    padding-top:0.35rem;
    margin-bottom: 0.2rem;
    position: relative;
}
.shopBargain ul li:nth-child(odd){
    margin-right: 0.2rem;
}
.shopBargain .tapImg{
    position: absolute;
    left: 0.2rem;
    top: -0.1rem;
    z-index: 10000;
}
.shopBargain .tapImg img{
    width: 1.15rem;
    height: 1.12rem;
}
.shopBargain .tapImg span{
    position: absolute;
    color: white;
    font-size: 0.36rem;
    left: 0.22rem;
    top: 0.2rem;
}
.shopBargain .img{
    width: 5.11rem;
    height: 5.11rem;
    position: relative;
}
.shopBargain .img img{
    width: 100%;
    height: 100%;
    margin: 0 auto;
}
.shopBargain .text h3{
    margin-top: 0.1rem;
    font-size: 0.48rem;
    color: #386998;
    font-weight: normal;
}
.shopBargain .text p span{
    font-size: 0.4rem;
    color: #386998;
}
.shopBargain .text p span strong{
    font-size: 0.6rem;
    color: #db6066;
}
.shopBargain .type{
    text-align: right;
    overflow: hidden;
}
.shopBargain .type span{
    font-size: 0.3401rem;
    color: white;
    padding: 0 0.25rem;
    margin-right: 0.1rem;
    border-radius: 0.1rem;
    height: 0.52rem;
    line-height: 0.52rem;
    display: inline-block;
}
.shopBargain .type span:first-child{
    background: #6fbee4;
}
.shopBargain .type span:nth-child(2){
    background: #de8cbd;
    margin-right: 0.2rem;
}
.advert img{
    width: 100%;
    height: 2.05rem;
}