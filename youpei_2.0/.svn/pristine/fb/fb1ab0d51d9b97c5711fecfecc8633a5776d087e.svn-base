*{
      margin: 0;
      padding: 0;
      font-size: 0.12rem;
    }
    li{
      list-style: none;
    }
    a{
      text-decoration: none;
    }
    input,textarea,select,button{
      outline: none;
    }
    img{
      border: none;
      display: block;
    }
    textarea{
      resize: none;
      overflow: hidden;
    }
    body,html{
      font-family: "微软雅黑";
    } 
    html{width: 100%;height: 100%;}
    .all{width: 100%;height: 100%;}
    .all >img{width: 100%;height: 100%;}
    .top{
      width: 100%;
      height: 100%;
    }
    /*头部样式*/
    .header {
      height: 6%;
      width: 100%;
      background: white;
      /*overflow: hidden;*/
      z-index: 1000;
      clear: both;
    }

    .header .back {
      left: 0.27rem;
      position: absolute;
    }

    .header .back img {
      margin-top: 0.4rem;
      height: 0.5rem;
    }

    .header h3 {
      text-align: center;
      font-size: 0.6rem;
      color: #065b9c;
      margin-left: 0.3rem;
      padding-top: 0.2rem;
    }

    .header .home {
      right: 0.2rem;
      position: absolute;
      top: 0rem;
      font-size: 0.6rem;
      color: #065b9c;
      text-decoration: none;
      font-weight: 500;
    }
    .home span{
      font-size: 0.8rem;
      font-weight: bold;
    }
    ul{
      position: absolute;
      width: 3rem;
      right: 0;
      bottom: inherit;
      z-index: 1000000;
      background: #f1f1f1;
      border-bottom-left-radius: 0.2rem;
      overflow: hidden;
    }
    .type li{
      background: white;
      width: 100%;
      text-align: center;
      font-size: 0.5rem;
      color: #065b9c;
      margin-top: 0.05rem;
      height: 1rem;
      line-height: 1rem;
      font-weight: 500;
    }
    .type li:nth-last-child(1){
      margin-bottom: 0;
    }
    .content{
      height: 94%;
      position: relative;
    }
    .swiper-container{
      width: 100%;
      height: 100%;
    }
    .swiper-wrapper{
      position: relative;
      /*margin-top: -0.5rem;*/
    }
    .desc{
      width: 100%;
      background: rgba(0,0,0,0.3);
      font-size: 0.45rem;
      /*text-align: center;*/
      text-indent: 2em;
      position: absolute;
      bottom: 0;
      left: 0;
      color: white;
      padding: 0.2rem 0rem;
    }
    p{
      width: 80%;
      margin:0 auto;
    }