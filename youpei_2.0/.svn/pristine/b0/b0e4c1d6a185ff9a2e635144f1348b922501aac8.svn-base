define(function(require, exports, module) {
  // 通过 require 引入依赖
    var $ = require('zepto');
 	if($('#shareContainer').length>0){
		$('#shareContainer').on('click',function(){
			$('#shareContainer').hide();
		});
		$('#btn-share').on('click',function(){
			$('#shareContainer').show();
		});
	}
});
