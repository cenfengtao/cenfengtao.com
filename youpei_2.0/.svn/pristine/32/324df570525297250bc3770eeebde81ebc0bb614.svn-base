body,html{
	background: #f1f1f1;
}

/*头部*/
.header {
	height: 1.2rem;
	line-height: 1.22rem;
	position: fixed;
	top: 0;
	width: 100%;
	background: white;
	overflow: hidden;
	z-index: 1000;
	clear: both;
}

.header .back {
	left: 0.27rem;
	position: absolute;
}

.header .back img {
	margin-top: 0.4rem;
	height: 0.5rem;
}

.header h3 {
	text-align: center;
	font-size: 0.6rem;
	color: #065b9c;
	margin-left: 0.3rem;
	margin-top: 0.05rem;
}

.header .home {
	right: 0.1rem;
	position: absolute;
	top: 0.05rem;
}

.header .home img {
	width: 1rem;
	height: 1rem;
}

/*中部*/
.img img{
	width: 100%;
	height: 4.73rem;
	margin-top: 1.3rem;
}
.img .text{
	height: 0.88rem;
	line-height: 0.88rem;
	font-size: 0.52rem;
	padding-left: 0.55rem;
	background: white;
	color: #416f9c;
	font-weight: bold;
}
.schedule{
	height: 1.08rem;
	line-height: 1.08rem;
	background: white;
	margin: 0.22rem 0rem;
	overflow: hidden;
}
.schedule img:first-child{
	margin-left: 0.64rem;
	margin-top: 0.18rem;
	width: 0.75rem;
	height: 0.72rem;
	float: left;
	margin-right: 0.2rem;
}
.schedule span{
	font-size: 0.4804rem;
	color: #386998;
	float: left;
	font-weight: bold;
}
.chose{
	height: 1.08rem;
	line-height: 1.08rem;
	background: white;
	margin: 0.22rem 0rem 0.02rem;
	overflow: hidden;
}
.chose img{
	margin-left: 0.64rem;
	margin-top: 0.18rem;
	width: 0.75rem;
	height: 0.72rem;
	float: left;
	margin-right: 0.2rem;
}
.chose span{
	font-size: 0.4804rem;
	color: #386998;
	float: left;
	font-weight: bold;
}

.choseNum{
	height: 1.08rem;
	line-height: 1.08rem;
	background: white;
	margin: 0.22rem 0rem 0.03rem;
	overflow: hidden;
}
.choseNum img{
	margin-left: 0.64rem;
	margin-top: 0.18rem;
	width: 0.75rem;
	height: 0.72rem;
	float: left;
	margin-right: 0.2rem;
}
.choseNum span{
	font-size: 0.4804rem;
	color: #386998;
	float: left;
	font-weight: bold;
}

.schedule img:nth-child(3){
	width: 0.2rem;
	height: 0.34rem;
	margin-top: 0.35rem;
	margin-right: 0.42rem;
	float: right;
}

.course{
	height: 1.35rem;
	background: white;
}

.num{
	margin-top: 0.25rem;
	padding-top: 0.22rem;
}
.num >div{
	border: 0.04rem solid #b5b5b5;
	width: 4.3rem;
	height: 0.82rem;
	margin: 0 auto;
	overflow: hidden;
	background: #eaeaea;
}
.num input{
	width: 1.7rem;
	height: 0.82rem;
	border-left: 0.04rem solid #b5b5b5;
	border-right: 0.04rem solid #b5b5b5;
	border-top:none;
	border-bottom: none;
	float: left;
	/*margin-top: -0.02rem;*/
	color: #386998;
	font-weight: bold;
	font-size: 0.72rem;
	text-align: center;
	background: white;
}
.reduce{
	background: #eaeaea;
	display: inline-block;
	float: left;
	height: 0.82rem;
	width: 1.23rem;
	text-align: center;
	line-height: 0.82rem;
	font-size: 0.72rem;
	color: black;
}
.add{
	background: #eaeaea;
	display: inline-block;
	height: 0.82rem;
	width: 1.23rem;
	text-align: center;
	line-height: 0.82rem;
	font-size: 0.72rem;
	color: black;
}


.type{
	height: 1.08rem;
	line-height: 1.08rem;
	background: white;
	margin-bottom: 0.2rem;
	padding-left: 0.64rem;
}
.type a{
	width: 2.15rem;
	height: 0.64rem;
	color: #777777;
	border: 0.04rem solid #777777;
	display: block;
	float: left;
	margin-right: 0.35rem;
	font-size: 0.4378rem;
	line-height: 0.7rem;
	text-align: center;
	border-radius: 0.1rem;
	margin-top: 0.2rem;
}
.type a.on{
	background: #386998;
	border-color: #386998;
	color: white;
}


.use{
	height: 1.08rem;
	line-height: 1.08rem;
	background: white;
	margin: 0.22rem 0rem 0.02rem;
	overflow: hidden;
}
.use img{
	margin-left: 0.64rem;
	margin-top: 0.18rem;
	width: 0.75rem;
	height: 0.72rem;
	float: left;
	margin-right: 0.2rem;
}
.use span{
	font-size: 0.4804rem;
	color: #386998;
	float: left;
	font-weight: bold;
}

.useImg{
	height: 3.04rem;
	background: white;
	padding-top: 0.35rem;
}
.useImg ul{
	padding: 0 0.4rem;
	display: flex;
	justify-content: space-between;
}
.useImg ul li img{
	width: 3.42rem;
	height: 2.5rem;
}


.pay{
	background: white;
	height: 3.6rem;
	margin-top: 0.22rem;
	padding-top: 0.5rem;
}
.pay .all{
	font-size: 0.55rem;
	text-align: center;
}
.pay .all span{
	text-decoration: line-through;
	font-size: 0.55rem;
}
.pay .really{
	color: #386998;
	font-size: 0.55rem;
	margin-top: 0.35rem;
	margin-bottom: 0.9rem;
	text-align: center;
	font-weight: bold;
}
.pay .really strong{
	color: #db6066;
	font-size: 0.55rem;
}
.rule{
	margin-bottom: 2.2rem;
	overflow: hidden;
	position: relative;
}
.rule i{
	width: 0.4rem;
	height: 0.4rem;
	border-radius: 50%;
	border: 0.01rem solid #46d2b0;
	margin-left: 1.8rem;
	display: block;
	top: 0.05rem;
	left: 0.1rem;
	position: absolute;
}
.rule input:checked+i{
	background: url(../../../../Public/images/checkbox.png) no-repeat center;
	background-size: 0.4rem 0.4rem;
}
.rule label{
	float: left;
	margin: 0 0 1rem 2.5rem;
	font-size: 0.4rem;
}
.rule label a{
	font-size: 0.4rem;
}

form{
	background: white;
	margin-top: 0.22rem;
	margin-bottom: 0.22rem;
}
form label{
	display: block;
	color: #386998;
	width: 90%;
	margin: 0 auto;
	font-size: 0.48rem;
}
form label span{
	font-size: 0.48rem;
	color: #386998;
}
form label input{
	font-size: 0.42rem;
	color: #8d8d8d;
	width: 6.89rem;
	height: 0.67rem;
	border: 0.03rem solid #c9c9c9;
	border-radius: 0.1rem;
	padding: 0 0.06rem;
}
form label select{
	font-size: 0.42rem;
	color: #8d8d8d;
	width: 2rem;
	height: 0.67rem;
	border: 0.03rem solid #c9c9c9;
	border-radius: 0.1rem;
	margin-right: 0.4rem;

}

form .name{
	height: 1.5rem;
	line-height: 1.5rem;
	border-bottom: 0.02rem solid #8d8d8d;
}
.name span{
	margin-left: 0.7rem;
}
form .tel{
	width: 90%;
	margin: 0 auto;
	height: 1.5rem;
	line-height: 1.5rem;
	border-bottom: 0.02rem solid #8d8d8d;
}
.tel span{
	margin-left: 0.2rem;
}

.address{
	margin-top: 0.35rem;
}
.address span{
	margin-left: 0.2rem;
}
.county{
	margin-right: 0rem!important;
}


.details{
	margin-top: 0.3rem;
	padding-bottom: 0.7rem;
}
.details span{
	float: left;
	margin-left: 1.4rem;
}

#remark {
    width:6.7rem;
    height:3.5rem;
    border:0.03rem solid #c9c9c9;
    border-radius:0.1rem;
    padding:0 0.06rem;
    margin-left:0.1rem;
    font-size:0.5rem;
    color:#8d8d8d;
}

::-webkit-input-placeholder { /* WebKit browsers */
color:#999;
}
:-moz-placeholder { /* Mozilla Firefox 4 to 18 */
color:#999;
}
::-moz-placeholder { /* Mozilla Firefox 19+ */
color:#999;
}
:-ms-input-placeholder { /* Internet Explorer 10+ */
color:#999;
}


.nextStep{
	position: relative;
}
.nextStep a{
	width: 100%;
	height: 1.5rem;
	line-height: 1.5rem;
	background: #cc585e;
	color: white;
	display: block;
	text-align: center;
	font-size: 0.55rem;
	font-weight: bold;
	/*padding-left: 4.2rem;*/
}
.nextStep a img{
	height: 0.68rem;
	width: 0.68rem;
	position: absolute;
	right: 4rem;
	top: 0.4rem;
}