}


.more{
	height: 1.06rem;
	line-height: 1.06rem;
	margin-top: 0.15rem;
	margin-bottom: 0.03rem;
	background: white;
	overflow: hidden;
}
.more img{
	width: 0.75rem;
	height: 0.72rem;
	margin-left: 0.6rem;
	margin-right: 0.2rem;
	float: left;
	margin-top: 0.18rem;
}
.more span{
	font-size: 0.48rem;
	color: #386998;
	float: left;
}
.more a{
	font-size: 0.48rem;
	float: right;
	margin-right: 0.38rem;
	color: #757575;
}



.img{
	background: white;
	padding: 0.27rem 0rem 0.36rem;
	overflow: hidden;
}

.img ul li{
	float: left;
	width: 50%;
}
.img img{
	width: 5.12rem;
	height: 3.22rem;
	margin:0 auto;
}
.img video{
	width: 5.12rem;
	height: 3.22rem;
	margin:0 auto;
}

.join a{
	height: 1.4rem;
	width: 7.1rem;
	display: block;
	margin: 0.76rem auto 0.64rem;
	background: #db6066;
	color: white;
	font-size: 0.63rem;
	line-height: 1.4rem;
	text-align: center;
	border-radius: 0.1rem;
	-webkit-border-radius:0.1rem;
	font-weight: bold;
}



.text{
	width: 10rem;
	margin: 0 auto;
	padding-bottom: 0.7rem;
}
.text p{
	font-size: 0.4rem;
	line-height: 0.65rem;
}
.text p span{
	font-size: 0.8rem;
	margin-right: 0.2rem;
	float: left;
}


.vote div p{
    border: 0.03rem solid #b5b5b5;
    float: left;
    width: 2.36rem;
    height: 0.6rem;
    margin: 0 0.42rem;
    overflow: hidden;
}
.vote input{
    width: 1.08rem;
    height: 0.6rem;
    border-left: 0.03rem solid #b5b5b5;
    border-right: 0.03rem solid #b5b5b5;
    border-top:none;
    border-bottom: none;
    float: left;
    /*margin-top: -0.02rem;*/
    color: #386998;
    font-weight: bold;
    font-size: 0.42rem;
    text-align: center;
}
.reduce{
    background: #eaeaea;
    display: inline-block;
    float: left;
    height: 0.6rem;
    width: 0.6rem;
    text-align: center;
    line-height: 0.6rem;
    font-size: 0.42rem;
    color: black;
}
.add{
    background: #eaeaea;
    display: inline-block;
    height: 0.6rem;
    width: 0.6rem;
    text-align: center;
    line-height: 0.6rem;
    font-size: 0.42rem;
    color: black;
    float: right!important;
}
.go{
    overflow: hidden;
    margin-bottom: 0.2rem;
}
.go a{
    display: block;
    width: 46.8%;
    margin: 0.2rem auto;
    text-align: center;
    background: #db6066;
    color: #fff;
    padding: 0.2rem;
    font-size: 0.5rem;
    border-radius: 5px;
}


.morePeople >span{
    position: absolute;
    right: 0.2rem;
    top: 1rem;
    font-size: 2rem;
    color: white;
    /*transform: rotate(45deg);*/
}
.morePeople{
    position: fixed;
    z-index: 10000000;
    height: 100%;
    width: 100%;
    background: rgba(0,0,0,0.7);
    top: 0;
    left: 0;
}
.morePeople img{
    height: 5rem;
    width: 6rem;
}

.tellBox{
    position: fixed;
    left: 0;
    top: 0;
    background: rgba(0,0,0,0.5);
    width: 100%;
    height: 100%;
    display: none;
    z-index: 1000000000;
}
 .tellContent{
    border-radius: 0.1rem;
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    height: 5.5rem;
    width:60%;
    margin: auto;
    background: white;
}
.tellContent >img{
    float: right;
    height: 0.47rem;
    width: 0.47rem;
    margin-top: 0.28rem;
    margin-right: 0.3rem;
}
.tellContent h3{
    text-align: center;
    font-size: 0.5rem;
    color: white;
    background: #386998;
    height: 1rem;
    line-height: 1rem;
    border-top-left-radius: 0.1rem;
    border-top-right-radius: 0.1rem;
}
.tellContent div{
	text-align: center;
    padding: 0.3rem;
    font-size: 0.4rem;
}
.tellContent a{
	background: #db6066;
    color: white;
    /* padding: 0.2rem 1rem; */
    border-radius: 0.1rem;
    width: 4.5rem;
    display: block;
    text-align: center;
    height: 0.8rem;
    line-height: 0.8rem;
    margin: 0 auto;
    font-size: 0.45rem;
}

.tellBoxs{
    position: fixed;
    left: 0;
    top: 0;
    background: rgba(0,0,0,0.5);
    width: 100%;
    height: 100%;
    z-index: 1000000000;
}
 .tellContents{
    border-radius: 0.1rem;
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    height: 6.5rem;
    width:60%;
    margin: auto;
    background: white;
}
.tellContents .closeBtn{
    float: right;
    height: 0.47rem;
    width: 0.47rem;
    margin-top: 0.28rem;
    margin-right: 0.3rem;
}
.tellContents h3{
    text-align: center;
    font-size: 0.5rem;
    color: white;
    background: #386998;
    height: 1rem;
    line-height: 1rem;
    border-top-left-radius: 0.1rem;
    border-top-right-radius: 0.1rem;
}
.tellContents div{
	text-align: center;
    padding: 0.3rem;
    font-size: 0.4rem;
}
.tellContents .code{
	width: 3rem;
	height: 3rem;
	margin: 0 auto;
}


/*轮播部分样式*/
 .arrow-left{
    width: 0.7rem;
    height: 1.4rem;
    border-radius: 0.2rem;
    background: rgba(216,216,0,0.7);
    font-size: 1rem;
    font-family: '宋体';
    line-height: 1.4rem;
    text-align: center;
    position: absolute;
    top:50%;
    margin-top: -0.7rem;
    color: #ffffff;
    z-index: 1000;
    left: 0.2rem;
}
.arrow-right{
    width: 0.7rem;
    height: 1.4rem;
    border-radius: 0.2rem;
    background: rgba(216,216,0,0.7);
    font-size: 1rem;
    font-family: '宋体';
    line-height: 1.4rem;
    text-align: center;
    position: absolute;
    top:50%;
    margin-top: -0.7rem;
    color: #ffffff;
    z-index: 1000;
    right: 0.2rem;
}
.pagination ,.swiper-pagination-clickable{
    width: 100%;
    text-align: center;
}
.swiper-pagination-bullet{
    margin-right: 0.2rem;
}
.swiper-pagination-bullet-active{
    background: #386998;
}

.sharePoster {
    display: block;
    width:50%;
    margin: 0 auto 0.3rem auto;
    text-align: center;
    background: #db6066;
    color:#fff;
    padding:0.2rem;
    font-size:0.5rem;
    border-radius:5px;
}

.desc p img{
    width: 100%!important;
    height: auto!important;
}

.explain{
    background: white;
    margin-top: 0.15rem;
    height: 1rem;
    line-height: 1rem;
    font-size: 0.5rem;
    padding-left: 0.2rem;
    color: #065b9c;
}


