.ty-ad-cover .title {
    display: block;
    background: #fff center no-repeat;
    width: 100%;
    position: absolute;
    bottom: 0;
    left: 0;
    height: 0;
    padding-bottom: 50%;
    background-size: 80% auto
}

.ty-ad-cover .close:before {
    display: block;
    font-size: 1.5rem;
    width: 2.5rem;
    height: 2.5rem;
    text-align: center;
    line-height: 2.5rem;
    position: absolute;
    left: 0;
    top: 0;
    z-index: 2;
    color: #fff
}

.ty-ad-cover .time {
    display: block;
    width: 1rem;
    height: 3rem;
    padding: 0 1rem;
    border-radius: 3rem;
    overflow: hidden;
    font-size: 1.5rem;
    text-align: center;
    line-height: 3rem;
    background: rgba(0, 0, 0, 0.5);
    color: #fff;
    position: absolute;
    right: .5rem;
    top: .5rem
}

.ty-ad-cover .time .num {
    animation: run 6s 1 steps(6);
    animation-fill-mode: forwards;
    display: block;
    font-style: normal;
    font-weight: 100;
    position: absolute;
    left: 1rem;
    width: 1rem;
    top: 0rem
}

.ty-ad {
    width: 100%;
    height: auto;
    margin: 0 0 1rem 0;
    position: relative
}

.ty-ad img {
    width: 100%;
    height: auto
}

.ty-ad .close {
    display: block;
    width: 2.5rem;
    height: 2.5rem;
    position: absolute;
    right: 0;
    top: 0;
    z-index: 2
}

@keyframes run {
    0% {
        top: 0rem
    }
    100% {
        top: -18rem
    }
}

@keyframes dis {
    0% {
        top: 0%
    }
    100% {
        top: -110%
    }
}

.mask {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: rgba(0, 0, 0, 0.8);
    display: none;
    text-align: center;
    color: #fff;
    z-index: 1
}

.mask .img-area img {
    width: 60vw;
    height: auto;
    margin: 5% auto;
    display: block
}

.mask .close {
    position: absolute;
    z-index: 2;
    top: 0;
    right: 0;
    display: block;
    width: 4rem;
    height: 4rem;
    color: #fff
}

.mask .close:before {
    font-size: 1.5rem;
    width: 4rem;
    height: 4rem;
    line-height: 4rem
}

/*appç™»å½•æ ·å¼*/
.ty-app-login {
    display: block;
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: 999;
    background: #fff;
    color: #299955
}

.ty-app-login .login-icon {
    position: relative;
    display: block;
    margin: 20% auto 0 auto;
    width: 7.5rem;
    height: 7.5rem;
    background: #299955;
    color: #fff;
    font-size: 5.75rem;
    line-height: 7.5rem;
    text-align: center;
    border-radius: 7.5rem
}

.ty-app-login .login-icon:before {
    margin: 0;
    font-size: 5.75rem
}

.ty-app-login .login-footer {
    position: fixed;
    margin: 1rem 0 0 -6rem;
    bottom: 5rem;
    left: 50%;
    width: 12rem;
    text-align: center;
    line-height: 2.5rem
}

.ty-app-login h2 {
    font-size: .875rem
}

.ty-app-login .btn-app-login {
    display: block;
    width: 10rem;
    text-align: center;
    box-shadow: 0 0 .25rem rgba(0, 0, 0, .25);
    margin: 1rem auto;
    border-radius: .25rem;
    background: #fff
}

.ty-app-login .btn-user-login {
    font-size: .875rem
}

.ty-app-login .btn-visitor-login {
    font-size: .875rem;
    color: #999;
    margin: .5rem auto;
    display: block
}

.ty-app-login .btn-visitor-login:before, .ty-app-login .btn-user-login:before {
    display: none
}

.ty-user-login {
    width: 10rem;
    margin: 0 auto;
    display: none
}

/*.ty-user-login.on{display:block}*/
.ty-user-login input {
    display: block;
    height: 2rem;
    line-height: 2rem;
    padding: .25rem;
    border: none;
    border-bottom: 1px solid rgba(0, 0, 0, .25);
    -webkit-appearance: none;
    appearance: none;
    border-radius: 0;
    outline: none;
    margin: 0;
    width: 9.5rem
}

.ty-user-login input:-webkit-autofill {
    -webkit-box-shadow: 0 0 0px 1000px white inset;
    box-shadow: 0 0 0px 1000px white inset
}

.ty-user-login .btn {
    background: #299955;
    color: #fff;
    width: 10rem;
    margin: 1rem 0;
    border-radius: .25rem;
    box-shadow: 0 0 .25rem rgba(0, 0, 0, .25);
    display: block
}

/*qrcode show*/
.ty-qrcode-area {
    position: fixed;
    width: 100%;
    height: 100%;
    left: 0;
    top: 0;
    z-index: 9001;
    background: rgba(0,0,0,.5);
}

.ty-qrcode-area .qrcode {
    display: block;
    width: 15rem;
    height: 17.5rem;
    position: fixed;
    left: 50%;
    top: 50%;
    margin: -7.5rem auto auto -7.5rem;
    text-align: center;
    line-height: 2.5rem;
    color: #fff;
    text-shadow: 1px 2px .25rem rgba(0, 0, 0, 0.3);
    border-radius: .25rem;
    background: #3eaedc;
    box-shadow: 1px 2px .25rem rgba(0, 0, 0, 0.3);
    padding: 1.25rem;
}

.ty-qrcode-area .qrcode img {
    display: block;
    box-shadow: 1px 2px .25rem rgba(0, 0, 0, 0.3);
    border-radius: .25rem;
    margin: 0 auto;
    width: 100%;
    height: auto
}

