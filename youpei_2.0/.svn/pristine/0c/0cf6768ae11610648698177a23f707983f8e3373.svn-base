/*å¼¹çª—æ ·å¼*/
.ty-pop-win {
    display: block;
    width: 100vw;
    height: 100vh;
    left: 0;
    background: rgba(0, 0, 0, 0);
    z-index: 1000;
    position: fixed;
    transition-duration: .4s;
    -webkit-transition-duration: .4s;
    pointer-events: none
}

.ty-pop-win.on {
    background: rgba(0, 0, 0, 0.5)
}

.pop-content {
    position: absolute;
    pointer-events: auto;
    width: 70vw;
    max-width: 420px;
    z-index: 1000;
    height: 90vw;
    max-height: 540px;
    top: 4rem;
    left: 0;
    right: 0;
    margin: auto;
    margin-top: 100vh;
    box-shadow: 0 0 .5rem rgba(0, 0, 0, .5);
    border-radius: .75rem;
    transition-duration: .4s;
    -webkit-transition-duration: .4s
}

.ty-pop-win.on .pop-content {
    margin-top: 0vh
}

.ty-pop-win img {
    width: 100%;
    height: auto;
    border-radius: .5rem
}

.ty-pop-win .desc {
    padding: 1rem;
    color: #ffffff;
    font-size: .875rem
}

.ty-pop-win .btn {
    position: absolute;
    bottom: 0;
    width: 100%;
    background: none;
    display: block;
    color: #fff;
    height: 90vw;
    max-height: 540px;
    border-radius: .25rem;
    text-align: center;
    left: 0;
    cursor: pointer;
    z-index: 10
}

.ty-pop-win .close {
    position: absolute;
    bottom: -4.5rem;
    right: 50%;
    margin-right: -1.25rem;
    background: rgba(255, 255, 255, 1);
    border-radius: 2.5rem;
    display: block;
    width: 2.5rem;
    height: 2.5rem;
    color: #999;
    line-height: 2.5rem;
    text-align: center;
    box-shadow: 0 0 .5rem rgba(0, 0, 0, .5);
    z-index: 1000;
    cursor: pointer
}

.ty-pop-win .close:before {
    margin-right: 0
}

.ty-pop-win .close:after {
    height: 1.75rem;
    display: block;
    content: "";
    border-right: 1px dashed #fff;
    right: 1.25rem;
    position: absolute;
    top: -1.75rem
}

.ty-jx {
    line-height: 1.5rem;
    text-align: center;
    color: #999;
    font-size: .75rem
}

.ty-jx:before, .ty-jx:after {
    content: "";
    display: inline-block;
    vertical-align: middle;
    width: 10%;
    height: 1px;
    background: #ccc;
    line-height: 0;
    margin: 0 1rem
}

