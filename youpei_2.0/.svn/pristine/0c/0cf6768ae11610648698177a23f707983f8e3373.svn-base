    height: 3rem;
    text-align: center;
    border-top: 1px dotted #ccc
}

.ty-wx-rec .tools .btn-close, .ty-wx-rec .tools .btn-go {
    display: inline-block;
    color: #888;
    background: #e5e5e5;
    height: 2rem;
    line-height: 2rem;
    margin: .5rem 2%;
    border-radius: .25rem;
    width: 36%
}

.ty-wx-rec .tools .btn-go {
    background: #eb647e;
    color: #fff
}

.ty-tip-more {
    position: fixed;
    right: 0;
    bottom: 5rem;
    border-radius: .25rem 0 0 .25rem;
    width: 1rem;
    display: block;
    text-align: center;
    line-height: 1.2em;
    z-index: 1000;
    animation: nextmove 1s infinite ease;
    -webkit-animation: nextmove 1s infinite ease;
    pointer-events: none;
    padding: .25rem 0 .25rem 0;
    color: #fff;
    background: #eb647e;
    font-size: .75rem;
    box-shadow: 0 0 .25rem rgba(0, 0, 0, .3)
}

.ty-tip-more b {
    display: inline-block;
    margin: 0 auto;
    line-height: 1.2em;
    font-weight: normal
}

.ty-tip-more:before {
    content: "";
    display: none
}

.ty-tip-more:after {
    display: block;
    margin: 0 auto;
    text-align: center
}

@keyframes nextmove {
    0% {
        transform: translateY(15px)
    }
    50% {
        transform: translateY(12px)
    }
    100% {
        transform: translateY(18px)
    }
}

@-webkit-keyframes nextmove {
    0% {
        -webkit-transform: translateY(15px);
        transform: translateY(15px)
    }
    50% {
        -webkit-transform: translateY(12px);
        transform: translateY(12px)
    }
    100% {
        -webkit-transform: translateY(18px);
        transform: translateY(18px)
    }
}

