/* transition 20170215 updated */
.banner-roll .item, .banner-nav i, .talent-roll .item, .talent-nav i, .ty-pop-win, .pop-content, .ty-cat-tags, .ty-cat-tags .tag, .ty-cat-tags .show-all, .ty-wx-rec, .ty-order-message, .talent-card .avatar, .act-tag, .cat-tag, .cat-tag-wrap, .activity.ty-calendars, .activity .ty-calendar, .activity .ty-calendar .day, .ticket.ty-calendars, .ticket .ty-calendar, .ticket .ty-calendar .day, .ty-profile-form ul.select, .ty-order-person .add-person, .ty-ad-cover {
    -webkit-transition: all .2s cubic-bezier(0, 0, .25, 1);
    transition: all .2s cubic-bezier(0, 0, .25, 1)
}

/* :after */
.list-tags .item:after, .ty-tip-more:after, .ty-detail-cover .startoff .icon-date-more:after, .ty-detail-content a.btn-continue:after, .ty-detail-content a.btn-custom:after, .ty-detail-content a.btn-share:after, .list-money .desc:after, .list-point .desc:after, .list-voucher .desc:after, .list-profile .item:after, .list-profile-del .item:after, .ty-app-login .btn-visitor-login:after, .ty-app-login .btn-user-login:after {
    content: attr(data-icon);
    font-family: "iconfont" !important;
    font-style: normal;
    -webkit-font-smoothing: antialiased;
    -webkit-text-stroke-width: 0.2px;
    -moz-osx-font-smoothing: grayscale;
    font-weight: normal;
    display: inline-block
}

/* æ®µåŽåˆ†å‰²çº¿ä¸­ç‚¹ */
.article:after, .ty-hot-search:after, .join-list .sum:after, .fav-list .sum:after, .box:after {
    content: "";
    display: block;
    margin: 0 auto -1rem auto;
    width: .25rem;
    height: .25rem;
    border-radius: .25rem;
    background-color: #ddd;
    border: .25rem solid #f0f1f2
}

/* avatar */
.avatar img {
    display: block;
    width: 100%;
    height: 100%;
    border-radius: 6rem;
    /*background: url(../images/icon_img.png) center 50% no-repeat #fff;*/
