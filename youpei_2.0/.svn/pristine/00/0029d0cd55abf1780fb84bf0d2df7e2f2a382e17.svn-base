/*头部 start*/
.header{
    height: 1.2rem;
    line-height: 1.22rem;
    position: fixed;
    width: 100%;
    background: white;
    overflow: hidden;
    z-index: 100;
    clear: both;
}
.header a:first-child{
    left: 0.27rem;
    position: absolute;
}
.header a:first-child img{
    margin-top: 0.4rem;
    height: 0.5rem;
}
.header h3{
    text-align: center;
    font-size: 0.52rem;
    color: #065b9c;
    margin-left: 0.3rem;
    margin-top: 0.05rem;
}
/*头部 end*/
/*中部 start*/
.middle{
    padding-top: 1.2rem;
    background: #f0f1f1;
}
.baseInfo{
    background: white;
}
.baseInfo >h3{
    font-size: 0.468rem;
    color: #368998;
    font-weight: normal;
    text-align: center;
    height: 1.3rem;
    line-height: 1.3rem;
    margin-top: 0.14rem;
    background: white;
}
.baseInfo >h3 strong{
    font-size: 0.468rem;
    color: #386998;
    font-weight: normal;
}

.works{
    /*height: 1.45rem;*/
    line-height: 1.45rem;
    width: 96%;
    overflow: hidden;
    margin-top: -0.1rem;
    margin: -0.1rem auto;
    border-top: 0.01rem solid #f1f1f1;
}
.works >span{
    font-size: 0.48rem;
    margin-left: 0.33rem;
    float: left;
    /*width: 85px;*/
    text-align: right;
    color: gray;
}
.works label >span{
    font-size: 0.48rem;
    margin-left: 0.33rem;
    float: left;
    /*width: 85px;*/
    text-align: right;
    color: gray;
}
.works input{
    width: 7rem;
    height: 0.88rem;
    line-height: 0.9rem;
    padding: 0 0.3rem;
    color: #4791c0!important;
    background: white;
    border-radius: 0.1rem;
    -webkit-border-radius:0.1rem;
    margin-top: -0.1rem;
    float: left;
    border: 0.01rem solid gray;
    margin-top: 0.2rem;
    font-size: 0.45rem;
    background: url(../../../../Public/images/pull-down.png) no-repeat right center;
    background-size: 0.53rem 1.13rem;
}

.works >img{
    width: 0.69rem;
    height: 0.74rem;
    float: right;
    margin-right: 0.3rem;
    margin-top: 0.25rem;
}

.logisticalInfo{
    background: white;
    margin-top: 0.15rem;
}
.logisticalInfo >h3{
    font-size: 0.468rem;
    color: #368998;
    font-weight: normal;
    text-align: center;
    height: 1.3rem;
    line-height: 1.3rem;
    margin-top: 0.2rem;
    background: white;
}
.logisticalInfo >h3 strong{
    font-size: 0.468rem;
    color: #386998;
    font-weight: normal;
}

.button span{
    width: 7.11rem;
    height: 1.41rem;
    line-height: 1.41rem;
    display: block;
    margin: 1.5rem auto 0.5rem;
    background: #db6066;
    color: white;
    font-size: 0.5rem;
    text-align: center;
    border-radius: 0.1rem;
}
.explain{
    color: #686868;
    text-align: center;
    font-size: 0.36rem;
    width: 7.56rem;
    margin: 0 auto 1.5rem;
}


/*上课地址样式*/
form label select{
    font-size: 0.42rem;
    color: #8d8d8d;
    height: 1.02rem;
    border: 0.03rem solid #386998;
    border-radius: 0.1rem;
   background: url(../../../../Public/images/pull-down.png) no-repeat right center;
    background-size: 0.53rem 1.13rem;
    appearance: none;
    -moz-appearance: none;
    -webkit-appearance: none;
    overflow: hidden;
    padding-left: 0.1rem;
    padding-right: 0.5rem;
}
form label select option{
    font-size: 0.42rem;
}
form .classTimes select{
    width: 2.42rem;
}
form .classAddress select{
        width: 2.5rem;
}
form .classAddress input{
    margin-left: 2.7rem;
}


/*日历*/
.date{
    background: #386998;
    background: url(../../../../Public/images/scheduleBg.jpg) no-repeat;
    background-size: 100% 100%;
    overflow: hidden;
    position: absolute;
    right: 0.65rem;
    margin-top: 1.1rem;
}
.date .dateHead{
    overflow: hidden;
    position: relative;
}
.date .dateHead .calendar_month_prev{
    position: absolute;
    color: white;
    font-size: 0.5rem;
    font-family: "宋体";
    font-weight: bolder;
    line-height: 1rem;
    height: 1rem;
    width: 0.7rem;
    left: 1.2rem;
}
.date .dateHead .calendar_month_next{
    position: absolute;
    color: white;
    font-size: 0.5rem;
    font-family: "宋体";
    font-weight: bolder;
    line-height: 1rem;
    height: 1rem;
    width: 0.7rem;
    right: 1.2rem;
}
.date .explain{
    font-size: 0.5rem;
    text-align: center;
    color: white;
    padding: 0 0.5rem;
    margin-bottom: 0.5rem;
}
.title{
    height: 1rem;
    line-height: 1rem;
    text-align: center;
    color: white;
    font-size: 0.48rem;
    font-weight: normal;
}
.day{
    width: 7.2rem;
    margin: 0 auto;
    height: 0.93rem;
    background: #6fbee4;
    border-top-right-radius: 0.1rem;
    border-top-left-radius: 0.1rem;
}
.day ul{
    display: flex;
    justify-content: space-around;
}
.day ul li{
    float: left;
    height: 0.93rem;
    line-height: 0.93rem;
}
.day ul li a{
    color: white;
    font-size: 0.4rem;
}
.list{
    width: 7.65rem;
    margin: 0 auto;
    margin-top: 0.05rem;
    padding-bottom: 0.28rem;
}
.list .schedule-bd{
    width: 7.2rem;
    margin:0 auto;
}
.list .schedule-bd{
    overflow: hidden;
}
.list .schedule-bd li{
    float: left;
    overflow: hidden;
}
.list .schedule-bd li {
    font-size: 0.48rem;
    border:0.01rem solid #386998;
    border-radius: 0.2rem;
    -moz-border-radius: 0.2rem;
    -webkit-border-radius: 0.2rem;
    height: 1.026rem;
    width: 1.026rem;
    background: white;
    text-align: center;
    position: relative;
    float: left;
    line-height: 1.026rem;
    display: block;
    box-sizing: border-box;
    
}
.list .schedule-bd li span{
    width: 100%;
    height: 100%;
    display: block;
}
.list .schedule-bd li span{
    font-size: 0.48rem;;
}
.list .schedule-bd li .red{
    height: 0.5rem;
    width: 0.5rem;
    border-radius: 50%;
    background: red;
    display: inherit;
    position: absolute;
    right: 0;
    margin: 0.05rem;
}
.list .schedule-bd li .gray{
    height: 0.5rem;
    width: 0.5rem;
    border-radius: 50%;
    background: gray;
    display: inherit;
    position: absolute;
    right: 0;
    margin: 0.05rem;
}
.list table tr td.active{
    background-color: #ffb400!important;
}