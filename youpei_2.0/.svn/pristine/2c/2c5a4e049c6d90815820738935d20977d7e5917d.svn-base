/*头部 start*/
.header{
    height: 1.2rem;
    line-height: 1.22rem;
    position: fixed;
    width: 100%;
    background: white;
    overflow: hidden;
    z-index: 100;
    clear: both;
}
.header a:first-child{
    left: 0.27rem;
    position: absolute;
}
.header a:first-child img{
    margin-top: 0.4rem;
    height: 0.5rem;
}
.header h3{
    text-align: center;
    font-size: 0.52rem;
    color: #065b9c;
    margin-left: 0.3rem;
    margin-top: 0.05rem;
}
/*头部 end*/
/*中部 start*/
.middle{
    padding-top: 1.2rem;
    background: #f0f1f1;
}
.baseInfo{
    background: white;
}
.baseInfo >h3{
    font-size: 0.468rem;
    color: #368998;
    font-weight: normal;
    text-align: center;
    height: 1.3rem;
    line-height: 1.3rem;
    margin-top: 0.14rem;
    background: white;
}
.baseInfo >h3 strong{
    font-size: 0.468rem;
    color: #386998;
    font-weight: normal;
}

.works{
    /*height: 1.45rem;*/
    line-height: 1.45rem;
    width: 96%;
    overflow: hidden;
    margin-top: -0.1rem;
    margin: -0.1rem auto;
    border-top: 0.01rem solid #f1f1f1;
}
.works span{
    font-size: 0.48rem;
    margin-left: 0.33rem;
    float: left;
    /*width: 85px;*/
    text-align: right;
    color: gray;
}
.works input{
    width: 6.2rem;
    height: 0.88rem;
    line-height: 0.9rem;
    padding: 0 0.3rem;
    color: #4791c0!important;
    background: white;
    border-radius: 0.1rem;
    -webkit-border-radius:0.1rem;
    margin-top: -0.1rem;
    float: left;
    border: 0.01rem solid gray;
    margin-top: 0.2rem;
    font-size: 0.45rem;
}

.works img{
    width: 0.69rem;
    height: 0.74rem;
    float: right;
    margin-right: 0.3rem;
    margin-top: 0.25rem;
}

.logisticalInfo{
    background: white;
    margin-top: 0.15rem;
}
.logisticalInfo >h3{
    font-size: 0.468rem;
    color: #368998;
    font-weight: normal;
    text-align: center;
    height: 1.3rem;
    line-height: 1.3rem;
    margin-top: 0.14rem;
    background: white;
}
.logisticalInfo >h3 strong{
    font-size: 0.468rem;
    color: #386998;
    font-weight: normal;
}

.button span{
    width: 4rem;
    height: 1rem;
    line-height: 1rem;
    display: block;
    margin: 1.5rem auto;
    background: #db6066;
    color: white;
    font-size: 0.5rem;
    text-align: center;
    border-radius: 0.1rem;
}
}