.go{
	background: white;
	padding: 0.55rem 0 0.55rem;
}
.go a{
	height: 1.17rem;
	width: 5.02rem;
	display: inline-block;
	margin: 0rem auto;
	color: white;
	font-size: 0.55rem;
	line-height: 1.17rem;
	text-align: center;
	border-radius: 0.1rem;
	-webkit-border-radius:0.1rem;
	font-weight: bold;
	margin-left: 0.4rem;
}
.vote1 a:first-child{
	background: #83cf95;
}
.vote1 a:nth-child(2){
	margin-top: 0.55rem;
	background: #db6066;
}
.vote2 a:first-child{
	background: #386998;
}
.vote2 a:nth-child(2){
	background: #db6066;
}
.morePeople{
	position: fixed;
	z-index: 10000;
	height: 100%;
	width: 100%;
	background: rgba(0,0,0,0.7);
	top: 0;
	left: 0;
}
.morePeople >span{
	position: absolute;
	right: 0rem;
	top: 1.11rem;
	font-size: 2rem;
	color: white;
	/*transform: rotate(45deg);*/
}

.morePeople img{
	height: 6rem;
	width: 8rem;
}

/*轮播部分样式*/
.arrow-left{
    width: 0.7rem; 
    height: 1.4rem; 
    border-radius: 0.2rem; 
    background: rgba(216,216,0,0.7); 
    font-size: 1.5rem; 
    font-family: '宋体'; 
    line-height: 1.4rem; 
    text-align: center; 
    position: absolute; 
    top:50%; 
    margin-top: -0.7rem; 
    color: #ffffff;
    z-index: 1000;
    left: 0.2rem;
}
.arrow-right{
    width: 0.7rem; 
    height: 1.4rem; 
    border-radius: 0.2rem; 
    background: rgba(216,216,0,0.7); 
    font-size: 1.5rem; 
    font-family: '宋体'; 
    line-height: 1.4rem; 
    text-align: center; 
    position: absolute; 
    top:50%; 
    margin-top: -0.7rem; 
    color: #ffffff;
    z-index: 1000;
    right: 0.2rem;
}
.pagination ,.swiper-pagination-clickable{
    width: 100%;
    text-align: center;
}
.swiper-pagination-bullet{
    margin-right: 0.2rem;
}
.swiper-pagination-bullet-active{
    background: #386998;
}

