/*作品名称*/
.name{
	background: white;
}
.name p{
	font-size: 0.48rem;
	line-height: 1rem;
	width: 10.56rem;
	margin: 0 auto;
	border-bottom: 0.01rem solid #aeaeae;
}
.name p i{
	height: 0.23rem;
	width: 0.23rem;
	border-radius: 50%;
	-webkit-border-radius:50%;
	background: #4c4c4c;
	display: block;
	float: left;
	margin-top: 0.4rem;
	margin-right: 0.2rem;
}
.name p span{
	color: #386998;
	font-size: 0.48rem;
}

