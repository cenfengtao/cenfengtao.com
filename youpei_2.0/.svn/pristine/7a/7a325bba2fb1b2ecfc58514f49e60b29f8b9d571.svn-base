body,html{
	font-family: "微软雅黑";
    background-color: #f0f1f1;
}	

/*中部样式*/
.middle{
    margin-top: 1.44rem;
    padding-bottom: 1.7rem;
    overflow: hidden;
}

/*头像部分*/
.head{
	height: 4.56rem;
	background: url(../../../../Public/images/bg.jpg) no-repeat;
	background-size: 100% 4.56rem;
	overflow: hidden;
	margin-bottom: 0.2rem;
}
.headLeft{
	float: left;
    width: 50%;
    text-align: center;
    margin-top: 0.4rem;
    border-right: 0.04rem solid white;
    box-sizing: border-box;
}
.headLeft img{
	width: 2.38rem;
	height: 2.38rem;
	margin: 0.5rem 0 0.2rem 1.7rem;
	border-radius: 50%;
}
.headLeft span{
	font-size: 0.48rem;
	color: white;
}
.headRight{
	float: right;
	width: 50%;
	text-align: center;
}
.headRight img{
	width: 2.38rem;
	height: 2.38rem;
	margin: 0.9rem 0 0.2rem 1.7rem;
}
.headRight span{
	font-size: 0.48rem;
	color: white;
	margin-top: 0.35rem;
}
.ty-qrcode-area {
    position: fixed;
    width: 100%;
    height: 100%;
    left: 0;
    top: 0;
    z-index: 90010000;
    background: rgba(0,0,0,.5);
}
.ty-qrcode-area .qrcode {
    display: block;
    width: 5rem;
    height: 5.5rem;
    position: fixed;
    margin: auto;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    text-align: center;
    line-height: 2.5rem;
    color: #fff;
    text-shadow: 1px 2px 0.25rem rgba(0, 0, 0, 0.3);
    border-radius: .25rem;
    background: #3eaedc;
    padding: 1.25rem;
    font-size: 0.5rem;
}
.ty-qrcode-area .qrcode img {
    display: block;
    box-shadow: 1px 2px 0.25rem rgba(0, 0, 0, 0.3);
    border-radius: .25rem;
    margin: 0 auto;
    width: 100%;
    height: auto;
}
/*公共标题*/
.title{
	overflow: hidden;
	height: 1.2rem;
	line-height: 1.2rem;
	background: white;
	border-bottom:0.02rem solid #f0f1f1;
}
.title img{
	width: 0.75rem;
	height: 0.72rem;
	float: left;
	margin: 0.24rem 0.24rem 0 0.55rem;
}
.title span{
	float: left;
	font-size: 0.48rem;
	font-weight: bold;
	color: #386998;
}
.title a{
	float: right;
	font-size: 0.42rem;
	color: #5b5b5b;
	font-family: "宋体";
	margin-right: 0.5rem;
}


/*我的积分*/
.integral{
	background: white;
	margin-bottom: 0.2rem;
}
.integralText ul{
	display: flex;
	justify-content: space-around;
}
.integralText ul li{
	height: 1.16rem;
	line-height: 1.16rem;
	font-size: 0.48rem;
}
.integralNum span{
	font-size: 0.48rem;
	font-weight: bold;
	color: #386998;
}
.getIntegral,.useIntegral,.leaveIntegral{
	width: 2.29rem;
	height: 1.04rem;
	border-radius: 0.1rem;
	border:0.03rem solid #353535;
	line-height: 1.04rem;
	text-align: center;
	display: inline-block;
}
.getIntegral{
	margin-left: 0.8rem;
}
.integralNum .leaveIntegral{
	color: #cc585e;
}
.integralUse{
	display: flex;
	justify-content: space-around;
	padding: 0.54rem 0;
}
.integralUse a{
	display: inline-block;
	width: 5.02rem;
	height: 1.23rem;
	font-size: 0.48rem;
	font-weight: bold;
	text-align: center;
	line-height: 1.23rem;
	color: white;
	border-radius: 0.1rem;
}
.integralUse a:first-child{
	background: #386998;
}
.integralUse a:nth-child(2){
	background: #db6066;
}
.integral p{
	width: 90%;
    margin: 0 auto;
    font-size: 0.4rem;
    text-align: center;
    color: red;
    padding-bottom: 0.5rem;
}
/*获取更多积分弹框*/
.tellBox{
    position: fixed;
    left: 0;
    top: 0;
    background: rgba(0,0,0,0.5);
    width: 100%;
    height: 100%;
    z-index: 1000000000;
    display: none;
}
 .tellContent{
    border-radius: 0.1rem;
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    height: 8.7rem;
    width:60%;
    margin: auto;
    background: white;
}
.tellContent .closeBtn{
    float: right;
    height: 0.47rem;
    width: 0.47rem;
    margin-top: 0.28rem;
    margin-right: 0.3rem;
}
.tellContent h3{
    text-align: center;
    font-size: 0.5rem;
    color: white;
    background: #386998;
    height: 1rem;
    line-height: 1rem;
    border-top-left-radius: 0.1rem;
    border-top-right-radius: 0.1rem;
}
.tellContent .text p{
	font-size: 0.4rem;
	padding: 0 0.2rem;
	line-height: 0.8rem;

}
/*如何增加学友*/
.tellBoxs{
    position: fixed;
    left: 0;
    top: 0;
    background: rgba(0,0,0,0.5);
    width: 100%;
    height: 100%;
    z-index: 1000000000;
    display: none;
}
 .tellContents{
    border-radius: 0.1rem;
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    height: 4.9rem;
    width:60%;
    margin: auto;
    background: white;
}
.tellContents .closeBtn{
    float: right;
    height: 0.47rem;
    width: 0.47rem;
    margin-top: 0.28rem;
    margin-right: 0.3rem;
}
.tellContents h3{
    text-align: center;
    font-size: 0.5rem;
    color: white;
    background: #386998;
    height: 1rem;
    line-height: 1rem;
    border-top-left-radius: 0.1rem;
    border-top-right-radius: 0.1rem;
}
.tellContents .text{
	font-size: 0.4rem;
	padding: 0 0.2rem;
	line-height: 0.8rem;
	text-align: center;
}
/*我的任务*/
.myTasks{
	background: white;
	margin-bottom: 0.2rem;
}
.myTasks .situation{
	text-align: center;
	font-size: 0.4rem;
	margin-top: 0.5rem;
	color: #7c7c7c;
}
.myTasks .situation span{
	font-size: 0.4rem;
	color: #386998;
}
.myTasks ul{
	overflow-x:scroll;
	white-space: nowrap;
	padding: 0.53rem 0 0.65rem;
}
.myTasks ul li{
	width: 2.5rem;
	line-height: 3.38rem;
	height: 3.38rem;
	text-align: center;
	display: inline-block;
	border-radius: 0.1rem;
	background: #f0f0f0;
	margin: 0 0.3rem;
	position: relative;
}
.myTasks ul li span{
	display: block;
	text-align: center;
}
.myTasks ul li span:first-child{
	font-size: 0.42rem;
	color: white;
	display: block;
	width: 100%;
	height: 1.47rem;
	line-height: 1.47rem;
	border-radius: 0.1rem;
}
.myTasks ul li span:nth-child(2){
	font-size: 0.48rem;
	color: white;
	display: block;
	width: 1.3rem;
	height: 1.3rem;
	line-height: 1.3rem;
	border-radius: 50%;
	text-align: center;
	margin: 0 auto;
	margin-top: 0.22rem;
}
.myTasks ul li:first-child span{
	background: #1ed373;
}
.myTasks ul li:nth-child(2) span{
	background: #62b7e3;
}
.myTasks ul li:nth-child(3) span{
	background: #386998;
}
.myTasks ul li:nth-child(4) span{
	background: #973076;
}
.myTasks ul li:nth-child(5) span{
	background: orange;
}
.myTasks ul li:nth-child(5) span:first-child{
	line-height: 0.75rem;
}
.myTasks ul li .red{
	position: absolute;
	top: -0.3rem;
	right: -0.2rem;
	height: 0.42rem;
	width: 0.42rem;
	background: #db6066;
	border-radius: 50%;
}
.myTasks ul li .gray{
	position: absolute;
	top: -0.3rem;
	right: -0.2rem;
	height: 0.42rem;
	width: 0.42rem;
	background: url(../../../../Public/images/grayBox.png) center no-repeat;
	background-size: 0.42rem 0.42rem;
	border-radius: 50%;
}
/*字体打勾图片*/
/*.myTasks ul li i:before{
	top: -1.7rem;
	right: -0.5rem;
	position: absolute;
	font-size: 0.4rem;
}*/
/*我的活动*/
.trendsContent ul{
	display: flex;
    justify-content: space-evenly;
    background: white;
    padding: 0.2rem 0;
    margin-bottom: 0.2rem;
}
.trendsContent ul li{
	width: 23.8%;
	display: inline-block;
    box-sizing: border-box;
}
.trendsContent ul li img{
	width: 100%;
    height: auto;
    border-radius: 0.1rem;
    display: block;
}
/*我的电子券*/
.coupons{
	background: white;
}
.coupons ul{
	margin-top: 0.4rem;
	margin-bottom: 0.2rem;
	padding-bottom: 0.4rem;
}
.coupons ul li{
	position: relative;
}
.coupons ul .cash_coupon{
	width: 10rem;
	margin: 0 auto 0.4rem;
	height: 3rem;
	background-image: url(../../../../Public/images/used1.png);
	background-size: 10rem 3rem;
}
.coupons ul .coupon{
	width: 10rem;
	margin: 0 auto 0.4rem;
	height: 3rem;
	background-image: url(../../../../Public/images/used.png);
	background-size: 10rem 3rem;
}
.texts{
    overflow: hidden;
    width: 77%;
}
.texts >span{
    font-size: 0.5407rem;
    float: left;
    color: white;
    margin-left: 0.26rem;
    margin-top: 0.5rem;
}
.texts span strong{
    font-size: 1.1rem;
}
.cash_coupon .texts div{
    float: right;
    margin: 0.2rem 0.6rem 0 0.18rem;
}
.cash_coupon .texts div span{
    color: white;
    float: left;
    font-size: 0.6489rem;
    margin-bottom: 0.1rem;
}
.cash_coupon .texts div p{
    color: white;
    float: left;
    clear: both;
    font-size: 0.2426rem;
    height: 0.8rem;
    width: 3.5rem;
}
.coupon .texts div{
    float: right;
    margin: 0.2rem 0.6rem 0 0.18rem;
}
.coupon .texts div span{
    color: white;
    float: left;
    font-size: 0.6489rem;
    margin-bottom: 0.1rem;
}
.coupon .texts div p:nth-child(2){
    color: white;
    float: left;
    clear: both;
    font-size: 0.3785rem;
}
.coupon .texts div p:nth-child(3){
    color: white;
    float: left;
    clear: both;
    font-size: 0.2426rem;
    width: 3.5rem;
    height: 0.65rem;
}
.hr{
    width: 64%;
    height: 0.03rem;
    background: whtie;
    background: white;
    margin-left: 0.3rem;
    margin-top: 0.1rem;
    margin-bottom: 0.1rem;
}
.content >p{
    clear: both;
    font-size: 0.2704rem;
    color: white;
    width: 66%;
    margin-left: 0.3rem;
}
.coupons .img{
	position: absolute;
	top: -0.08rem;
	right: 3.4rem;
	height: 1.04rem;
	width: 1.15rem;

}
.coupons .imgTap{
	height: 1.04rem;
	width: 1.15rem;
}
.coupons .imgLogo{
	height: 0.69rem;
	width: 0.7rem;
	margin: auto;
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
}

/*我的钱包*/
.myPrice{
	background: white;
	margin-bottom: 0.2rem;
}
.myPrice ul{
	display: flex;
	justify-content: space-around;
}
.myPrice ul li{
	text-align: center;
	width: 33.333%;
	margin: 0.3rem 0;
}
.myPrice ul li:nth-child(2){
	border-right: 0.01rem solid #a9a9a9;
	border-left: 0.01rem solid #a9a9a9;
}
.myPrice ul li img{
	height: 1.25rem;
	width: 1.25rem;
	margin: 0 auto;
}
.myPrice ul li span{
	font-size: 0.36rem;
	color: #636363;
	display: block;
	margin-top: 0.17rem;
}
.myPrice ul li label{
	font-size: 0.3rem;
	color: #386998;
}
/*我的订单*/
.myOrder .panel{
	background: #fff;
    margin-bottom: .25rem;
    text-align: center;
    height: 100%;
    overflow: hidden;
    width: 100%;
    box-sizing: border-box;
}
.myOrder .panel .btn {
    display: inline-block;
    width: 25%;
    float: left;
    height: 2.4rem;
    font-size: .36rem;
    line-height: 0.36rem;
    position: relative;   
}
.myOrder .panel .btn:before{
    display: block;
    margin: 0 auto;
    font-size: 0.8rem;
    color: #999!important;
    margin:0.5rem 0;
}
.myOrder .panel label{
	font-size: .36rem;
    font-weight: normal;
    color: #386998;
}
/*我的学友*/
.classmates{
	background: white;
	overflow: hidden;
	margin-bottom: 0.2rem;
}
.classmates>a{
	width: 10.56rem;
	height: 1.15rem;
	line-height: 1.15rem;
	color: white;
	text-align: center;
	display: block;
	margin: 0.42rem auto;
	background: #db6066;
	font-size: 0.55rem;
	border-radius: 0.1rem;
}
.classmates p{
	margin: 0 0.42rem;
	line-height: 1.9rem;
	height: 1.9rem;
	border-top: 0.01rem solid #c8c8c8;
}
.classmates p span{
	font-size: 0.48rem;
	margin-left: 0.23rem;
}
.classmates p strong{
	font-size: 0.48rem;
	color: #386998;
}
.classmates p a{
	width: 2.15rem;
	height: 0.75rem;
	line-height: 0.75rem;
	text-align: center;
	border: 0.05rem solid #83cf95;
	color: #83cf95;
	font-size: 0.45rem;
	float: right;
	margin-right: 0.3rem;
	margin-top: 0.5rem;
}
/*我的优培*/
.mine{
	background: white;
}
.mine ul{
	display: flex;
	justify-content: space-around;
}
.mine ul li{
	text-align: center;
	position: relative;
}
.mine ul li img{
	width: 1.12rem;
	height: 1.12rem;
	margin: 0 auto;
	margin-bottom: 0.14rem;
}
.mine ul li span{
	font-size: 0.34rem;
	color: black;
}
.mine ul{
	padding: 0.22rem 0 0.35rem;
}
.item-new{
	position: absolute;
    top: -0.2rem;
    right: 0;
    width: 0.4rem;
    height: 0.4rem;
    line-height: 0.4rem;
    display: inline-block;
    text-align: center;
    background: #db6066;
    color: white!important;
    border-radius: 50%;
}
/*版权申明*/
.info{
	height: 0.9rem;
	line-height: 0.9rem;
	font-size: 0.36rem;
	text-align: center;
	width: 98%;
	margin: 0 auto;
	background: #6fbee4;
	margin-top: 0.57rem;
	color: white;
	margin-bottom: 1rem;
}