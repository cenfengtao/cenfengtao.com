/*头部 start*/
.header{
	height: 1.2rem;
	line-height: 1.2rem;
	position: relative;
}
.header a{
	float: left;
	margin-top: 0.36rem;
	margin-left: 0.38rem;
}
.header a img{
    height: 0.5rem;
}
.header h3{
	text-align: center;
	font-size: 0.52rem;
	color: #386998;
}
/*头部 end*/

/*中部 start*/
.img img{
	width: 100%;
	height: 4.72rem;
	border-top: 0.13rem solid #f0f1f1;
	border-bottom: 0.18rem solid #f0f1f1;
}

.classes{
	height: 9.42rem;
	background: #386998;
	width: 100%;
}
.week{
	height: 1rem;
	position: relative;
}
.week h3{
	text-align: center;
	font-size: 0.48rem;
	color: white;
	height: 1rem;
	line-height: 1rem;
}
.week h3 span{
	font-size: 0.48rem;
}
.week .left{
	position: absolute;
	width: 0.5rem;
	height: 0.5rem;
	background: white;
	color: #43719e;
	border-radius: 50%;
	font-size: 0.3rem;
	line-height: 0.5rem;
	text-align: center;
	font-weight: bolder;
	font-family: '宋体';
	left: 1.92rem;
	top: 0.28rem;
}
.week .right{
	position: absolute;
	right: 1.92rem;
	top: 0.28rem;
	width: 0.5rem;
	height: 0.5rem;
	background: white;
	color: #43719e;
	border-radius: 50%;
	font-size: 0.3rem;
	line-height: 0.5rem;
	text-align: center;
	font-weight: bolder;
	font-family: '宋体';
}
.day{
	width: 10.96rem;
	margin: 0 auto;
	height: 0.93rem;
	background: #6fbee4;
	border-top-right-radius: 0.08rem;
	border-top-left-radius: 0.08rem;
}
.day ul{
	display: flex;
	justify-content: space-around;
}
.day ul li{
	float: left;
	height: 0.93rem;
	line-height: 0.93rem;
}
.day ul li a{
	color: white;
	font-size: 0.4rem;
}
.list{
	width: 10.96rem;
	margin: 0 auto;
	margin-top: 0.05rem;
}
.list table tr td{
	/*border:0.01rem solid #386998;*/
	border-radius: 0.2rem;
	-moz-border-radius: 0.2rem;
	-webkit-border-radius: 0.2rem;
	height: 1.3rem;
	width: 1.3rem;
	background: white;
	text-align: center;
	position: relative;
}
#monitor button{
	display: inline-block;
  	font-size: inherit;
  	color: inherit;
  	border: none;
  	text-align: center;
  	background: #f8ef61;
  	height: 1.3rem;
	width: 1.3rem;
}
/*.mark1{
	background: #028BE0 !important;
	color:#fff;
	border-radius: 2px;
}*/
#monitor span{
	height: 1.3rem;
	width: 1.3rem;
}
.list table tr:first-child td{
	height: 1.3rem;
	background: #f8ef61;
	font-size: 0.4448rem;
	line-height: 1.3rem;
	text-align: center;
	width: 1.3rem;
}
.list table tr td:first-child{
	background: #f8ef61;
	font-size: 0.4316rem;
	text-align: center;
	color: #386998;
	height: 1.3rem;
	width: 1.3rem;
}
.list table tr td:first-child span{
	font-size: 0.4316rem;
	text-align: center;
	color: #386998;
	display:block;
	line-height: 0.1rem;
}
.light{
	margin-top: -0.1rem;
	float: left;
	margin-left: 0.47rem;
	font-size: 1rem;
	font-weight: 100;
}
.ke,.yu{
	width: 1.12rem;
	height: 1.12rem;
	border-radius: 50%;
	text-align: center;
	line-height: 1.12rem;
	display: inline-block;
	font-size: 0.528rem;
	color: white;
}
.ke{
	background: #71ccb0;
}
.yu{
	background: #f7d06a;
}
.smallKe{
	position: absolute;
	top: 0.04rem;
	left: 0.04rem;
	height: 0.64rem;
	width: 0.64rem;
	background: #71ccb0;
	color: white;
	font-size: 0.324rem;
	line-height: 0.64rem;
	text-align: center;
	border-radius: 50%;
}
.smallYu{
	position: absolute;
	bottom: 0.04rem;
	right: 0.04rem;
	height: 0.64rem;
	width: 0.64rem;
	background: #f7d06a;
	color: white;
	font-size: 0.324rem;
	line-height: 0.64rem;
	text-align: center;
	border-radius: 50%;
}
.alert{
	height: 3.85rem;
	background: #f0f1f1;
}
.alert p:first-child{
	padding-top: 0.8rem;
	margin-left: 2.1rem;
	font-size: 0.42rem;
	line-height: 0.9rem
}
.alert p:first-child span:first-child{
	height: 0.7rem;
	width: 0.7rem;
	border-radius: 50%;
	background: #db6066;
	color: white;
	font-size: 0.42rem;
	display: inline-block;
	line-height: 0.7rem;
	text-align: center;
}
.alert p:first-child span:nth-child(2){
	color: #db6066;
	font-size: 0.42rem;
	margin: 0 -0.15rem;
}
.alert p:nth-child(2){
	margin-left: 3.15rem;
	font-size: 0.42rem;
	line-height: 0.9rem;
}


/*弹框部分*/
.form{
	position: absolute;
	width: 100%;
	height: 100%;
	top: 0;
	left: 0;
	z-index: 10000;
	background: rgba(0,0,0,0.5);
	display: none;
}
form{
	position: absolute;
	top: 0;
	bottom: 0;
	left: 0;
	right: 0;
	margin:auto;
	width: 9.5rem;
	height: 9.76rem;
	background: white;
	border-radius: 0.2rem;
}
form label{
	font-size: 0.4rem;
}
form input{
	border: 0.01rem solid black;
	padding: 0.1rem 0.2rem;
	margin:0.3rem 0;
	height: 0.3rem;
	font-size: 0.3rem;
}

.head{
	height: 1.3rem;
	line-height: 1.3rem;
	background: #386998;
	overflow: hidden;
	width: 100%;
}
.apoint{
	width: 0.71rem;
	height: 0.68rem;
	float: left;
	margin: 0.34rem 0.15rem 0 0.4rem;
}
.head span{
	float: left;
	font-size: 0.5361rem;
	font-weight: bold;
	color: white;
}
.close{
	height: 0.47rem;
	width: 0.47rem;
	float: right;
	margin: 0.37rem 0.45rem 0 0;
}


.apointment{
	overflow: hidden;
}
.apointTime{
	font-size: 0.48rem;
	margin: 0.6rem 0 0 0.48rem;
	float: left;
	color: #686868;
}
.longTime{
	float: left;
	margin-top: 0.42rem;
	margin-left: 0.3rem;
	color: #686868;
	width: 65%;
}




.light1{
	border-top: 0.02rem solid #e7e7e7;
	width: 96%;
	margin: 0 auto;
}
.name{
	margin-left: 0.3rem;
	font-size: 0.48rem;
	color: #686868;
}
.tel{
	margin-left: 0.3rem;
	font-size: 0.48rem;
	margin-right: 0.5rem;
	color: #686868;
}
#name{
	width: 4.96rem;
	height: 0.96rem;
	border-radius: 0.1rem;
	border: 0.03rem solid #386998;
}
#tel{
	width: 5rem;
	width: 4.96rem;
	height: 0.96rem;
	border-radius: 0.2rem;
	border: 0.03rem solid #386998;
}
.time{
	margin-right: 0.1rem;
	height: 0.4rem;
	width: 0.4rem;
}
.submit{
	border:0.01rem solid #ef595e;
	width: 8.66rem;
	height: 1.04rem;
	text-align: center;
	font-size: 0.48rem;
	color: white;
	background: #ef595e;
	position: absolute;
	bottom: 0.17rem;
	border-radius: 0.1rem;
	margin: 0 auto;
	margin-left: 0.4rem;
}