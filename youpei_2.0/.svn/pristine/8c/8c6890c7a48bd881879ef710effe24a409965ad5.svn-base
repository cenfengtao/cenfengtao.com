<script>
//    获取url参数
function GetRequest() {
    var url = location.search; //获取url中"?"符后的字串
    var theRequest = new Object();
    if (url.indexOf("?") != -1) {
        var str = url.substr(1);
        strs = str.split("&");
        for (var i = 0; i < strs.length; i++) {
            theRequest[strs[i].split("=")[0]] = unescape(strs[i].split("=")[1]);
        }
    }
    return theRequest;
}
var vote_id=GetRequest().vote_id;
    $.ajax({
        type:"post",
        url:"/index.php/Vote/ajaxuserVote",
        async: true,
        dataType:"json",
        data:{
        },
        success:function(data){
            if(data.status==1){
                var data=data.data;
                var dataLength=data.length;
                for(var i=0; i<dataLength; i++){
                    if(data[i].upload_type==1){
                        $(".vote-list").append(
                            '<li onclick="toVote('+data[i].id+')">'+
                                '<div class="img">'+
                                     '<img src="'+data[i].path+'" alt="">'+
                                '</div>'+
                                '<p class="name">名称：<span>'+data[i].title+'</span></p>'+
                                '<p>作者：<span>'+data[i].username+'</span></p>'+
                                '<div class="text">'+
                                    '<span>共<strong>'+data[i].vote_count+'</strong>票</span>'+
                                    '<span class="vote">为TA投票</span>'+
                                '</div>'+
                            '</li>'
                        )
                    }
                    if(data[i].upload_type==2){
                        $(".vote-list").append(
                            '<li onclick="toVote('+data[i].id+')">'+
                                '<div class="img">'+
                                     '<video src="'+data[i].path+'" autoplay></video>'+
                                     // '<div class="play">'+
                                     //    '<img src="../../../../Public/images/play.png" alt="">'+
                                     // '</div>'+
                                '</div>'+
                                '<p class="name">名称：<span>'+data[i].title+'</span></p>'+
                                '<p>作者：<span>'+data[i].username+'</span></p>'+
                                '<div class="text">'+
                                    '<span>共<strong>'+data[i].vote_count+'</strong>票</span>'+
                                    '<span class="vote">为TA投票</span>'+
                                '</div>'+
                            '</li>'
                        )
                    }
                }
            }
            
        }
    })
function toVote(id){
        window.location.href="/index.php/Vote/voteDetail.html?id="+id+"&vote_id="+vote_id;
    }
</script>
