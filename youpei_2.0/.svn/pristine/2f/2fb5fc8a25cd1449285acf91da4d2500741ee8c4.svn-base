    /*商品列表样式*/
    .productContent{
        overflow: hidden;
        /*margin-bottom: 1.5rem;*/
    }
    .productContent ul{
        overflow: hidden;
        margin-right: -0.6rem;
    }
    .productContent ul li{
        float: left;
        width: 48%;
        background: white;
        margin-right: 0.25rem;
        margin-bottom: 0.25rem;
        position: relative;
    }
    .productContent .texts{
        margin: 0.18rem 0 0 0.57rem;
    }
    .productContent ul li .img{
        display: block;
    }
    .productContent ul li .img img{
        width: 100%;
        height: 9rem;
    }
    .productContent .address img{
        width: 0.33rem;
        height: 0.45rem;
        float: left;
        margin: 0 0.03rem;
    }
    .productContent .address span{
        float: left;
        font-size: 0.36rem;
        color: #757575;
        margin-top: -0.08rem;
    }
    .productContent .texts{
        padding-bottom: 0.2rem;
    }
    .productContent .texts h3{
        clear: both;
        color: #386998;
        font-size: 1rem;
        margin: 0.2rem 0;
        height: 2rem;
        overflow: hidden;
    }
    .productContent .describe{
        font-size: 0.8rem;
        width:9rem;
        white-space:nowrap;
        text-overflow:ellipsis;
        -o-text-overflow:ellipsis;
        overflow: hidden;
    }
    .productContent .type{
        overflow: hidden;
        margin-right: -0.15rem;
    }
    .productContent .type span{
        font-size: 0.34rem;
        height: 0.52rem;
        /*width: 1.4rem;*/
        padding:0 0.13rem;
        border-radius: 0.1rem;
        -webkit-border-radius: 0.1rem;
        /*text-align: center;*/
        line-height: 0.52rem;
        margin-right: 0.16rem;
        display: inline-block;
        color: white;
    }
    .productContent .type span:first-child{
        background: #f6d94f;
    }
    .productContent .type span:nth-child(2){
        background: #6fbee4;
    }
    .productContent .type span:nth-child(3){
        background: #de8cbd;
    }
    .productContent .original-price{
        color: #386998;
        text-decoration: line-through;
        margin-top: 0.3rem;
        float: left;
        font-size: 0.8rem;
    }
    .productContent .texts p{
        clear: both;
        line-height: 1.6rem;
    }
    .productContent .texts p span{
        font-weight: bold;
    }
    .productContent .texts p span:first-child{
        color: #386998;
        font-size: 0.8rem;
    }
    .productContent .texts p span:nth-child(2){
        color: #db6066;
        font-size: 0.8rem;
    }
    .productContent .texts p span:nth-child(3){
        color: #db6066;
        font-size: 1rem;
        margin-left: -0.1rem;
    }
    .productContent .texts p span:nth-child(4){
        color: #999999;
        font-size: 0.3877rem;
    }

