.m-hdlist{ width: 100%;height: 100%; margin: 0 auto;}
.m-main{  margin-top: 3rem;}
.title-g,.title-r,.title-b{
    background: #ffffff;
    margin-left: .5rem;
    border-bottom: 2px solid #f0f1f1;
    height: 2rem;
    display: block;
font-size: .9rem;
}
.title-g span{
    padding-left:.4rem; 
     margin-left: .4rem;
     font-weight: bold;
     line-height: 1.8rem;
     border-left: 4px solid #4c8f00;

}
.title-r span{
    padding-left:.4rem; 
     margin-left: .4rem;
     font-weight: bold;
     line-height: 1.8rem;
     border-left: 4px solid red;

}
.title-b span{
    padding-left:.4rem; 
     margin-left: .4rem;
     font-weight: bold;
     line-height: 1.8rem;
     border-left: 4px solid #386998;
}
.m-main ul li{
    float: left;
    margin-top: 0.5rem;
    width: 33%;
    padding-bottom: .5rem; 
   display: block;
}
ul.m-list{
     float:left;
     border-bottom: 3px solid #f0f1f1;
     width: 100%;
}
.m-main ul li .ico {
    height: 3rem;
    width: 3rem;
   margin: 0 auto;
}
.m-main ul li .fz16 {
text-align: center;
font-weight: normal;
font-size: .8rem;
}
.m-main img{

    height: 100%;
    width: 100%;
}
.m-main .red{
    color:red;
}
.m-main .blue{
    color:#386998;
}
.m-main .green{
    color:green;
}
.go{
  display: flex;
  justify-content: space-around;
}
.go li{
  text-align: center;
}
.go li a img{
  height: 108px;
  width: 108px;
  display: block;
  margin: 0 auto 30px;
}
.go li a span{
  color: white;
  background: brown;
  padding: 10px;
  border-radius: 8px;
}