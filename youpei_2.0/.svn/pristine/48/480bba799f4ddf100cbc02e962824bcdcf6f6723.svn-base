body,html{
	background: #f1f1f1;
}
/*头部*/
.header {
	height: 1.2rem;
	line-height: 1.22rem;
	position: fixed;
	top: 0;
	width: 100%;
	background: white;
	overflow: hidden;
	z-index: 1000;
	clear: both;
}

.header .back {
	left: 0.27rem;
	position: absolute;
}

.header .back img {
	margin-top: 0.4rem;
	height: 0.5rem;
}

.header h3 {
	text-align: center;
	font-size: 0.6rem;
	color: #065b9c;
	margin-left: 0.3rem;
	margin-top: 0.05rem;
}

.header .home {
	right: 0.27rem;
	position: absolute;
	top: 0.15rem;
}

.header .home img {
	width: 1rem;
	height: 1rem;
}


/*中部*/
.product{
	background: white;
	overflow: hidden;
	margin-bottom: 0.22rem;
}
.img{
	float: left;
	margin:0.51rem 0.34rem;
}
.img img{
	width: 5.54rem;
	height: 3.8rem;
}
.middle{
	margin-top: 1.3rem;
}
.content{
	float: left;
	position: relative;
	height: 4.9rem;
	width: 4.95rem;
}
.content h3{
	color: #386998;
	font-size: 0.6rem;
	margin-top: 0.8rem;
}
.content .original{
	position: absolute;
	left: 0;
	bottom: 1.7rem;
	font-size: 0.42rem;
	color: #4b4b4b;
	text-decoration: line-through;
}
.content .now{
	position: absolute;
	left: 0;
	bottom: 0.9rem;
}
.now span:first-child{
	color: #386998;
	font-size: 0.48rem;
	font-weight: bold;
}
.now span:nth-child(2){
	font-size: 0.4rem;
	color: #db6066;
}
.now span:nth-child(3){
	font-size: 0.6rem;
	color: #db6066;
	font-weight: bold; 
}
.now span:nth-child(4){
	font-size: 0.4rem;
	color: #386998;
	font-weight: bold;
}




.way{
	background: white;
	margin-bottom: 0.22rem;
}
.tag{
	position: relative;
}
.tag img{
	height: 0.81rem;
	width: 3.06rem;
}
.tag span{
	color: white;
	position: absolute;
	left: 0.4rem;
	top: 0.12rem;
	font-size: 0.4804rem;
}
.rule{
	margin-left: 1.38rem;
	margin-top: 0.4rem;
	padding-bottom: 1.1rem;
}
.rule p{
	font-size: 0.42rem;
	color: #585858;
	line-height: 0.68rem;
}


.rate{
	background: white;
	padding-bottom: 0.74rem;
	margin-bottom: 0.22rem;
}
.rate .title{
	position: relative;
}
.rate .title h3{
	width: 100%;
	text-align: center;
	height: 1.37rem;
	line-height: 1.37rem;
	font-size: 0.52rem;
	color: #386998;
}
.rate .title span{
	position: absolute;
	border: 0.04rem solid #386998;
	width: 1.14rem;
	left: 3.24rem;
	top: 0.67rem;
}
.rate .title span:nth-child(3){
	left: 7rem;
}
.light{
	position: relative;
	width: 10.6rem;
	height: 0.87rem;
	border: 0.06rem solid #386998;
	margin-left: 0.35rem;
	border-radius: 0.48rem;
	-webkit-border-radius: 0.48rem;
	overflow: hidden;
}
.green{
	border-right: 0.06rem solid #386998;
	height: 0.87rem;
	position: absolute;
	background: #71ccb0;
	width: 9rem;
	border-radius: 0.48rem;
	-webkit-border-radius: 0.48rem;
	z-index: 1000;
	text-align: center;
	color: white;
	line-height: 0.9rem;
	font-size: 0.48rem;
}
.gray{
	height: 0.87rem;
	position: absolute;
	background: gray;
	width: 10.7rem;
	-webkit-border-radius: 0.48rem;
	border-radius: 0.48rem;
	z-index: 100;
	line-height: 0.9rem;
}
.gray span{
	float: right;
	color: white;
	padding-right: 0.5rem;
	font-size: 0.48rem;
}



.alert{
	background: white;
	height: 0.95rem;
	line-height: 0.95rem;
}
.alert img{
	width: 0.56rem;
	height: 0.56rem;
	float: left;
	margin-left: 0.38rem;
	margin-top: 0.22rem;
	margin-right: 0.2rem;
}
.alert span{
	font-size: 0.42rem;
}
.alert span strong{
	color: #e49195; 
	text-decoration: underline;
	font-size: 0.7rem;
	font-weight: bold;
}
.alert a{
	font-size: 0.42rem;
	font-weight: bold;
	text-decoration: underline;
	color: #436e98;
	margin-left: 0.35rem;
}


.buy a{
	width: 7.11rem;
	height: 1.41rem;
	text-align: center;
	line-height: 1.41rem;
	background: #db6066;
	font-size: 0.63rem;
	color: white;
	margin: 1.8rem auto;
	display: block;
	border-radius: 0.1rem;
	-webkit-border-radius:0.1rem;
}

.together a:first-child{
	width: 7.11rem;
	height: 1.41rem;
	text-align: center;
	line-height: 1.41rem;
	background: #db6066;
	font-size: 0.63rem;
	color: white;
	margin: 0.7rem auto;
	display: block;
	border-radius: 0.1rem;
	-webkit-border-radius:0.1rem;
}
.together a:nth-child(2){
	width: 7.11rem;
	height: 1.41rem;
	text-align: center;
	line-height: 1.41rem;
	background: #f4b014;
	font-size: 0.63rem;
	color: white;
	margin: 0 auto 1.5rem;
	display: block;
	border-radius: 0.1rem;
	-webkit-border-radius:0.1rem;
}
.box{
	position: fixed;
	z-index: 10000;
	height: 100%;
	width: 100%;
	background: rgba(0,0,0,0.5);
	top: 0;
	left: 0;
}
.box .content{
	height: 7.96rem;
	width: 8.92rem;
	border-radius: 0.1rem;
	-webkit-border-radius:0.1rem;
	position: fixed;
	top:0;
	bottom: 0;
	left: 0;
	right: 0;
	margin: auto;
	background: white;
}
.box .content .top{
	height: 1.3rem;
	width: 100%;
	background: #386998;
	line-height: 1.3rem;
	border-top-left-radius: 0.1rem;
	border-top-right-radius: 0.1rem;
}
.top{
	overflow: hidden;
}
.top img:first-child{
	height: 0.65rem;
	width: 0.64rem;
	float: left;
	margin-left: 0.35rem;
	margin-top: 0.32rem;
	margin-right: 0.3rem;
}
.top img:nth-child(3){
	float: right;
	height: 0.56rem;
	width: 0.43rem;
	margin-top: 0.32rem;
	margin-right: 0.35rem;
}
.box .content .top span{
	font-size: 0.5361rem;
	color: white;
}
.box .content .reduce p{

	width: 3.8rem;
	margin: 0 auto;
	text-align: center;
	font-size: 0.6rem;
	margin-top: 1.58rem;
	margin-bottom: 1.4rem;
}
.reduce p span{
	font-size: 0.6rem;
	color: #809ab7;
}
.box .content >a{
	height: 1.19rem;
	width: 5.09rem;
	text-align: center;
	line-height: 1.19rem;
	display: block;
	color: white;
	background: #db6066;
	font-size: 0.5929rem;
	margin: 0 auto;
	border-radius: 0.15rem;
}
.share{
	position: fixed;
	z-index: 10000;
	height: 100%;
	width: 100%;
	background: rgba(0,0,0,0.7);
	top: 0;
	left: 0;
}
.share >span{
	position: absolute;
	right: 0.7rem;
	top: 0;
	font-size: 2rem;
	color: white;
	transform: rotate(45deg);
}
.share .step{
	color: white;
	height: 3rem;
	position: absolute;
	top: 0;
	bottom: 0;
	left: 0;
	right: 0;
	margin: auto;
	width: 80%;
}
.step p{
	margin-bottom: 0.3rem;
	font-size: 0.5rem;
	line-height: 0.8rem;
}
.step p:first-child{
	font-size: 0.8rem;
}
.morePeople{
	position: fixed;
	z-index: 10000;
	height: 100%;
	width: 100%;
	background: rgba(0,0,0,0.7);
	top: 0;
	left: 0;
}
.morePeople p{
	text-align: center;
	color: white;
	height: 3rem;
	position: absolute;
	top: 0;
	bottom: 0;
	left: 0;
	right: 0;
	margin: auto;
	width: 80%;
	font-size: 0.5rem;
}

/*底部*/
.footer{
    clear: both;
    position: fixed;
    bottom: 0;
    width: 100%;
}
.icon{
    height: 1.5rem;
    background: #386998;
    height: 1.5rem;
    z-index: 100;
}
.footer ul{
    display: flex;
    justify-content: space-around;
}
.footer ul li{
    height: 1.5rem;
    line-height: 1.6rem;
    position: relative;
}
.footer ul li>img{
    display: none;
}
.footer ul li >img{
    position: absolute;
    top: 0;
    right: 0;
    left: 0;
    margin: 0 auto;
    width: 0.42rem;
    height: 0.21rem;
    display: block;
}
.footer ul li a span{
    display: inline-block;
    font-size: 0.4rem;
    color: white;
    float: left;
}
.footer ul li a img{
    display: inline-block;
    height: 0.5rem;
    float: left;
    margin-top: 0.5rem;
    margin-right: 0.05rem;
}
.footer div{
    height: 0.2rem;
    background: #f0f1f1;
}