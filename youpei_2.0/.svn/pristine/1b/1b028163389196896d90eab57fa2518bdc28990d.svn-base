.footer ul li a img{
	margin-top: 0.52rem;
}
.footer ul li a span{
	margin-left: 0.14rem;
}
body,html{
	background: #f1f1f1;
}
.middle{
	padding-top: 1.2rem;
	/*padding-bottom: 1.6rem;*/
}

.img{
	margin-top: 0.1rem;
	margin-bottom: 0.01rem;
}
.title h3{
	height: 0.87rem;
	line-height: 0.87rem;
	background: white;
	font-size: 0.52rem;
	padding-left: 0.37rem;
	color: #416f9c;
	margin-bottom: 0.05rem;
}
.classTime{
	height: 1.08rem;
	line-height: 1.08rem;
	background: white;
	margin-bottom: 0.05rem;
}
.finished{
	font-size: 0.5rem;
	color: #4f4e4e;
}
.finished strong{
	font-size: 0.5rem;
	color: #4f4e4e;
	margin-left: 0.33rem;
}

.classTime>strong{
	font-size: 0.5rem;
	color: #416f9c;
}
.classTime .num{
	font-size: 0.5rem;
	color: #416f9c;
	margin-right: 1.35rem;
	float: right;
    font-weight: bold;
}


.credit{
	height: 1.08rem;
	line-height: 1.08rem;
	background: white;
	margin-bottom: 0.05rem;
}
.mark{
	font-size: 0.5rem;
	color: #4f4e4e;
}
.mark strong{
	font-size: 0.5rem;
	color: #4f4e4e;
	margin-left: 0.33rem;
}

.credit label{
	display: block;
}
.credit label>strong{
	font-size: 0.5rem;
	color: #416f9c;
}
#nowIntegral{
	font-size: 0.5rem;
}
.credit .reduceNum{
	font-size: 0.5rem;
	color: #416f9c;
	float: right;
    font-weight: bold;
}
.credit input{
	float: right;
}
.check{
	float: right;
    height: 0.76rem;
    width: 0.76rem;
    border-radius: 0.1rem;
    display: inline-block;
    background: url(../../images/border.png) no-repeat center; 
    background-size: 0.76rem 0.76rem;
    margin: 0.16rem 0.35rem 0 0.28rem;
}
.credit input:checked+i{
    background: url(../../images/checked.png) no-repeat center;
    background-size: 0.76rem 0.76rem;
}


.roll{
	height: 1.08rem;
	line-height: 1.08rem;
	background: white;
	margin-bottom: 0.05rem;
}
.rolls{
	font-size: 0.5rem;
	color: #4f4e4e;
}
.rolls strong{
	font-size: 0.5rem;
	color: #4f4e4e;
	margin-left: 0.33rem;
}

.roll>strong{
	font-size: 0.5rem;
	color: #4f4e4e;
	float: right;
	font-family: "宋体";
	margin:0 0.6rem 0 0.5rem;
	font-weight: bolder;
}
.roll .rollNum{
	font-size: 0.5rem;
	color: #416f9c;
	float: right;
    font-weight: bold;
}
.rollNum .money{
	font-size: 0.5rem;
}
.coupon-lists li{
	overflow: hidden;
	margin-bottom: 0.6rem;
}
.coupon-lists li:first-child{
	margin-top: 0.6rem;
}
.coupon-lists .content{
    background: url(../../../../Public/images/used.png) no-repeat;
    background-size: 100% 100%;
    height: 2.72rem;
    width: 9.36rem;
    margin-left: 0.43rem;
    display: block;
    float: left;
}
.coupon-lists .contents{
    background: url(../../../../Public/images/used1.png) no-repeat;
    background-size: 100% 100%;
    height: 2.72rem;
    width: 9.36rem;
    margin-left: 0.43rem;
    margin-bottom: 0.6rem;
    display: block;
    float: left;
}
.layui-m-layerchild{
    background: #f1f1f1;
}


.texts{
    overflow: hidden;
    width: 72%;
}
.texts >span{
    font-size: 0.5407rem;
    float: left;
    color: white;
    margin: 0.2rem 0 0.29rem 0.2rem;
}
.texts span strong{
    font-size: 1rem;
}
.texts div{
    float: right;
    margin-top: 0.2rem;
}
.texts div span{
    color: white;
    float: left;
    font-size: 0.55rem;
}
.texts div p{
    color: white;
    float: left;
    clear: both;
    font-size: 0.3785rem;
    height: 1rem;
    width: 3.5rem;
}
.hr{
    width: 64%;
    height: 0.04rem;
    background: whtie;
    background: white;
    margin-left: 0.36rem;
}
.content >p{
    clear: both;
    font-size: 0.2704rem;
    color: white;
    width: 66%;
    margin-top: 0.1rem;
    margin-left: 0.5rem;
}
.contents >p{
    clear: both;
    font-size: 0.2704rem;
    color: white;
    width: 66%;
    margin-top: 0.1rem;
    margin-left: 0.5rem;
}
.contents .texts div{
	float: left;
    font-size: 0.8rem;
    margin-left: 1rem;
    margin-top: 0.4rem;
}
.contents .texts div span{
	font-size: 0.7rem;
}

.coupon-lists label{
	display: block;
	overflow: hidden;
}
.coupon-lists  .check{
	float: right;
    height: 0.76rem;
    width: 0.76rem;
    border-radius: 0.1rem;
    display: inline-block;
    background: url(../../images/border.png) no-repeat center; 
    background-size: 0.76rem 0.76rem;
    margin: 1rem 0.35rem 0 0.28rem;
}
.coupon-lists input:checked+i{
    background: url(../../images/checked.png) no-repeat center;
    background-size: 0.76rem 0.76rem;
}


.use{
	height: 2.5rem;
	text-align: center;
	margin-bottom: 0.2rem;
	width: 90%;
	margin: 0 auto;
	color: #416f9c;
	font-size: 0.45rem;
	margin-bottom: 0.5rem;
}
.information{
	height: 2.06rem;
	background: white;
	margin-bottom: 0.2rem;
	color: #4f4e4e;
}
.information p{
	clear: both;
	float: left;
	margin-left: 0.34rem;
}
.information p span{
	font-size: 0.5rem;
	color: #4f4e4e;
}
.information p span:nth-child(2){
	margin-left: 0.2rem;
}
.information p span strong{
	font-size: 0.5rem;
	color: #416f9c;
}
.information p:first-child{
	float: left;
	margin:0.3rem 0 0.1rem 0.34rem;
}
.point{
	color: #4f4e4e!important;
}


.order{
	background: white;
	height: 1.5rem;
	line-height: 1.5rem;
	text-align: center;
}

.order span{
	font-size: 0.52rem;
	font-weight: bold;
	color: #4f4e4e;
}
.order span strong{
	font-size: 0.52rem;
	color: #cc585e;
}
#totalPrice,#realPrice{
	color: #cc585e;
}
.buy button{
	width: 100%;
	height: 1.5rem;
	line-height: 1.5rem;
	text-align: center;
	background: #cc585e;
	color: white;
	font-size: 0.52rem;
	float: right;
	display: inline-block;
}