    white-space: nowrap;
}
.nav ul li{
    /*float: left;*/
    display: inline-block;
    width: 20%;
    text-align: center;
    margin-bottom: 0.4rem;
}
.nav ul li img{
    margin: 0 auto;
    width: 1.12rem;
    height: 1.12rem;
