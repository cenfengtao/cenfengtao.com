/*导航样式*/
.fixed{
    position: fixed;
    top: 0rem;
    left: 0;
    z-index: 55500000;
}
.nav{
    background-color: white;
    overflow: hidden;
    margin-top: 0.12rem;
    margin-bottom: 0.18rem;
}
.nav ul{
    padding-top: 0.46rem;
    padding-bottom: 0.06rem;
