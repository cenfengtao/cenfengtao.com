    text-align: center;
    height: 1.22rem;
    line-height: 1.22rem;
}
.mapContent >h3 strong{
    font-size: 0.5rem;
    color: #386998;
    font-weight: normal;
}
.map{
    display: inline-block;
    width: 100%;
    height: 6.5rem;
    z-index: 10000;
    position: absolute;
}
.searchOrg{
    position: absolute;
    right: 2.5%;
    top: 0.4rem;
    width: 1rem;
    height: 1rem;
    z-index: 100;
}
.report{
    position: absolute;
    right: 2.5%;
