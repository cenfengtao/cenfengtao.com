    line-height: 1.5rem;
    font-family: verdana
}

.btn-share, .share-invite {
    display: none !important
}

.app .btn-share, .app .share-invite {
    display: block !important
}

a.btn-share {
    position: absolute;
    right: 0;
    top: 0;
    color: #444;
    width: 2.5rem
}

.ty-iframe {
    width: 100vw;
    height: 100%;
    position: fixed;
    left: 0;
    top: 0;
    padding-top: 2.5rem;
    box-sizing: border-box;
    background: #fff;
    overflow-y: auto;
    z-index: 10;
    -webkit-overflow-scrolling: touch
}

@media screen and (min-width: 960px) {
    html {
        /*width: 700px;*/
