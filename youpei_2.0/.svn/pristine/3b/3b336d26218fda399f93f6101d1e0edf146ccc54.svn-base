@charset "utf-8";
@media screen and (max-width: 639px) {
    html {
        font-size: 16px;
    }
}

@media screen and (min-width: 640px) and (max-width: 768px) {
    html {
        font-size: 32px;
    }
}

@media screen and (min-width: 769px) {
    html {
        font-size: 36px;
    }
}

@-webkit-keyframes shake {
    0% {
        -webkit-transform: translateY(-.3125rem);
        transform: translateY(-.3125rem)
    }
    50% {
        -webkit-transform: translateY(0);
        transform: translateY(0)
    }
    100% {
        -webkit-transform: translateY(-.3125rem);
        transform: translateY(-.3125rem)
    }
}

.ani-shake {
    -webkit-animation: shake .5s infinite;
    animation: shake .5s infinite
}

@font-face {
    font-family: "iconfont";
    src: url('/Public/fonts/iconfont.eot');
    src: url('/Public/fonts/iconfont.svg') format('svg'),
    url('/Public/fonts/iconfont.eot') format('embedded-opentype'),
    url('/Public/fonts/iconfont.ttf') format('truetype'),
    url('/Public/fonts/iconfont.woff') format('woff');
}

@font-face {
    font-family: "character";
    src: url('/Public/fonts/character.OTF');
}

.iconfont, [data-icon]:before {
    font-family: "iconfont" !important;
    font-size: 1rem;
    font-style: normal;
    -webkit-font-smoothing: antialiased;
    -webkit-text-stroke-width: .2px;
    -moz-osx-font-smoothing: grayscale
}

.kap-input-value, [data-icon-blue]:before {
    font-family: "iconfont" !important;
    font-size: 1rem;
    font-style: normal;
    -webkit-font-smoothing: antialiased;
    -webkit-text-stroke-width: .2px;
    -moz-osx-font-smoothing: grayscale;
    content: attr(data-icon-blue);
    display: inline-block;
    margin-left: .25rem;
    color: #236ca1;
}

[data-icon]:before {
    content: attr(data-icon);
    display: inline-block;
    margin-right: .25rem
}

.introduce nobr{width:94.1%;
    float: left;
    overflow: hidden;}

[data-icon-after]:after {
    font-family: "iconfont" !important;
    font-size: 1rem;
    font-style: normal;
    -webkit-font-smoothing: antialiased;
    -webkit-text-stroke-width: .2px;
    -moz-osx-font-smoothing: grayscale;
    content: attr(data-icon-after);
    display: inline-block;
    margin-left: .25rem
}
.introduce [data-icon-after]:after {
    transform:rotate(90deg);
    float: right;
}

input::-webkit-inner-spin-button, input::-webkit-outer-spin-button {
    -webkit-appearance: none
}

::-webkit-search-cancel-button {
    display: none
}

[data-ty-target], [data-ty-url], [class*="btn"] {
    cursor: pointer
}

/* 统一要用到的样式 */
/* transition */
.ty-menu-top, .ty-menu-bottom, .ty-page, .ty-announce, #btn_top {
    -webkit-transition: all .2s cubic-bezier(0, 0, .25, 1);
    transition: all .2s cubic-bezier(0, 0, .25, 1)
}

/* 工具条尖角 */
.ty-tools-idx a:after, .ty-tools-user a:after {
    content: " ";
    display: block;
    width: 1.25rem;
    height: 1.25rem;
    background-color: #f0f1f2;
    position: absolute;
    left: 50%;
    top: -1.5625rem;
    border-radius: .25rem;
    -webkit-transform: rotate(45deg);
    transform: rotate(45deg);
    -webkit-transition: all .2s cubic-bezier(0, 0, .25, 1);
    transition: all .2s cubic-bezier(0, 0, .25, 1)
}

/* 工具栏图标大小 */
.ty-tools a:before {
    font-size: 1.25rem
}

/* 穿透效果 */
.ty-menu-top {
    pointer-events: none;
}

.ty-menu-top * {
    pointer-events: auto;
}

/* 水平滚动 20170308 updated */
.ty-sc-h {
    padding: .5rem 1rem;
    background: #fff;
    margin-bottom: .25rem;
    overflow-y: hidden;
    overflow-x: scroll;
    white-space: nowrap;
    height: auto;
    width: 100%;
    text-align: center;
    -webkit-overflow-scrolling: touch;
    padding-bottom: .5rem;
}

.ty-sc-h::-webkit-scrollbar {
    display: none;
}

.ty-sc-h > * {
    display: inline-block;
    width: 40vw;
    margin-right: .5rem
}

.kap-loading-icon {
    border: none;
    -webkit-animation: kap_ani_rotate .5s infinite linear;
    animation: kap_ani_rotate .5s infinite linear;
    width: 3rem;
    height: 3rem;
    position: fixed;
    z-index: 9999;
    left: 50%;
    top: 50%;
    display: none;
    margin-left: -2rem;
    margin-top: -3rem;
}

 .ty-loading-icon {
    border: none;
    width: 3rem;
    height: 3rem;
    position: fixed;
    z-index: 9999;
    left: 10%;
    top: 40%;
    display: none;
    margin-left: -1rem;
    margin-top: 1rem
}

.kap-loading .kap-loading-icon {
    display: block
}

.kap-loading .ty-loading-icon {
    display: block
}

.kap-loading:before {
    background-color: rgba(255, 255, 255, 0.95);
    z-index: 9999
}

/*20160831 updated*/
.kap-wrap {
    position: relative;
    margin-left: 0vw;
    width: 100vw;
    -webkit-transition: all 0s;
    transition: all 0s
}

.refresh-page {
    position: fixed;
    top: 50%;
    margin-top: 3rem;
    line-height: 2.5rem;
    left: 50%;
    margin-left: -3.5rem;
    width: 7rem;
    color: #999;
    text-align: center;
    -webkit-animation: delay_hide 0.2s 1 linear;
    animation: delay_hide 0.2s 1 linear;
    -webkit-animation-fill-mode: forwards;
    animation-fill-mode: forwards;
    display: block;
    opacity: 0;
    pointer-events: none
}

.kap-loading-tran .refresh-page {
    opacity: 0;
    -webkit-animation: delay_show 4s 1 linear;
    animation: delay_show 4s 1 linear;
    -webkit-animation-fill-mode: forwards;
    animation-fill-mode: forwards;
    pointer-events: auto;
}

.kap-loading-tran .ty-tools-current {
    display: none
}

.kap-loading-tran .kap-wrap {
    margin-left: -100vw;
    -webkit-transition: all .2s linear;
    transition: all .2s linear
}

.kap-loading-tran .ty-tools-loading {
    display: block;
    z-index: 9999
}

#page_loading {
    display: block;
    position: fixed;
    left: 105vw;
    -webkit-transition: all 0s;
    transition: all 0s;
    pointer-events: none
}

.kap-loading-tran #page_loading {
    background: #f0f1f2;
    z-index: 400;
    left: 0vw;
    -webkit-transition: all .2s linear;
    transition: all .2s linear;
    pointer-events: auto;
}

@-webkit-keyframes delay_show {
    0% {
        opacity: 0
    }
    99% {
        opacity: 0
    }
    100% {
        opacity: 1
    }
}

@keyframes delay_show {
    0% {
        opacity: 0
    }
    99% {
        opacity: 0
    }
    100% {
        opacity: 1
    }
}

@-webkit-keyframes delay_hide {
    0% {
        opacity: 0
    }
    100% {
        opacity: 0
    }
}

@keyframes delay_hide {
    0% {
        opacity: 0
    }
    100% {
        opacity: 0
    }
}

