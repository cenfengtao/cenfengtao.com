}

/* 公告 */
.ty-announce {
    position: fixed;
    left: 0;
    top: 2.5rem;
    width: 100%;
    height: 3.125rem;
    background-color: #fc0;
    color: #222;
    line-height: 3.125rem;
    font-size: 1rem;
    text-align: left;
    z-index: 8888;
    box-shadow: 0 .25rem .3125rem rgba(0, 0, 0, 0.3)
}

.ty-announce .content {
    padding: 0 2.5rem 0 1.25rem;
    height: 100%;
    box-shadow: 0 .125rem .25rem rgba(0, 0, 0, 0.1) inset;
    overflow: hidden;
    text-overflow: ellipsis
}

.ty-announce .btn-close {
    position: absolute;
    right: 0;
    top: 0
}

.ty-announce .btn-close:before {
    font-size: 1.25rem;
    margin-right: .125rem;
    float: left;
    width: 2.5rem;
    text-align: center;
    text-indent: 0;
    opacity: .5
}

.ty-announce a {
    color: #222;
    text-decoration: none
}

.ty-view-all .ty-announce, .ty-view-top .ty-announce {
    top: 2.5rem
}

