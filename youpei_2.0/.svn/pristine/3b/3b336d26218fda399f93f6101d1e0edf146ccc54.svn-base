/* html正文 */
.text {
    font-size: .875rem;
    line-height: 1.075rem;
    color: #222
}

.text p {
    padding-bottom: .9375rem;
    text-align: justify
}

.text p span {
    font-size: .875rem !important
}

.text img {
    display: block;
    width: 100% !important;
    height: auto !important;
    box-shadow: .0625rem .0625rem .5rem rgba(0, 0, 0, 0.3);
    /*background: url(../images/icon_img.png) center 50% no-repeat #f0f1f2;*/
    background-size: 4.6875rem auto;
    max-width: 100% !important;
}

.text strong {
    color: #299955;
    font-weight: bold;
    font-size: .875rem !important
}

.text h3, .text h4, .text h5 {
    border-left: .25rem solid #ff8000;
    padding-left: .625rem;
    font-weight: bold;
    margin: 0 0 .9375rem 0;
    font-size: 1rem
}

.text h4 {
    border-left: .25rem solid #8C8C8C
}

.text h5 {
    border-left: .25rem solid #00BCDB
}

.text h6 {
    font-size: 1rem;
    font-weight: normal;
    color: #888;
    margin-bottom: .9375rem
}

.text ul {
    margin-bottom: .9375rem
}

.text li {
    padding-left: 1.125rem;
    position: relative;
    border-bottom: 1px solid #dddddd;
    margin-bottom: .3125rem;
    padding-bottom: .3125rem
}

.text li:before {
    content: " ";
    display: block;
    width: .375rem;
    height: .375rem;
    border-radius: .375rem;
    background-color: #ccc;
    position: absolute;
    left: .3125rem;
    top: .5rem
}

.text li:last-child {
    margin-bottom: 0;
    padding-bottom: 0;
    border-bottom: none
}

.text a {
    margin: 0 .25rem;
    text-decoration: none;
    color: #299955
}

.text a:before {
    display: none
}

.text a:after {
    content: "\150";
    font-family: "iconfont" !important;
    font-size: .875rem;
    font-style: normal;
    -webkit-font-smoothing: antialiased;
    -webkit-text-stroke-width: .2px;
    -moz-osx-font-smoothing: grayscale;
    line-height: .875rem;
    margin-left: .25rem
}

.text a[href^="tel"]:after {
    display: none
}

.text a[href^="tel"]:before {
    display: inline-block;
    content: "\140";
    font-family: "iconfont" !important;
    font-size: .875rem;
    font-style: normal;
    -webkit-font-smoothing: antialiased;
    -webkit-text-stroke-width: .2px;
    -moz-osx-font-smoothing: grayscale;
    line-height: .875rem;
    margin-right: .25rem
}

.text summary {
    padding-left: .625rem;
    font-weight: bold;
    margin: 0 0 .9375rem 0;
    font-size: 1rem
}

.text summary::-webkit-details-marker {
    color: #ff8000
}

/*20170204 updated*/
.text center {
    margin-bottom: 1rem;
    font-size: .875rem;
    color: #999
}

/*20170204 updated end*/

.text-pics {
    position: relative;
    margin-bottom: .9375rem;
    width: 100%;
    overflow: hidden;
    background-color: #ddd;
    box-shadow: .0625rem .0625rem .5rem rgba(0, 0, 0, 0.3)
}

.text-pics .pics {
    position: absolute;
    height: 100%;
    left: 0;
    top: 0
}

.text-pics .pics img {
