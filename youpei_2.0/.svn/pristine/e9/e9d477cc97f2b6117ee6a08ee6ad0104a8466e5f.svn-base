/*头部 start*/
.header{
    height: 1.2rem;
    line-height: 1.22rem;
    position: fixed;
    width: 100%;
    background: white;
    overflow: hidden;
    z-index: 100;
    clear: both;
}
.header a{
    left: 0.27rem;
    position: absolute;
}
.header a img{
    margin-top: 0.4rem;
    height: 0.5rem;
}
.header h3{
    text-align: center;
    font-size: 0.52rem;
    color: #065b9c;
    margin-left: 0.3rem;
    margin-top: 0.05rem;
}
/*头部 end*/
/*中部 start*/
.middle{
    padding-top: 1.2rem;
    background: #f0f1f1;
}
.date{
    background: #386998;
}
.title{
    height: 1rem;
    line-height: 1rem;
    text-align: center;
    color: white;
    font-size: 0.48rem;
    margin-top: 0.1rem;
}
.day{
    width: 10.96rem;
    margin: 0 auto;
    height: 0.93rem;
    background: #6fbee4;
    border-top-right-radius: 0.08rem;
    border-top-left-radius: 0.08rem;
}
.day ul{
    display: flex;
    justify-content: space-around;
}
.day ul li{
    float: left;
    height: 0.93rem;
    line-height: 0.93rem;
}
.day ul li a{
    color: white;
    font-size: 0.4rem;
}
.list{
    width: 10.96rem;
    margin: 0 auto;
    margin-top: 0.05rem;
    padding-bottom: 0.28rem;
}
.list table{
    width: 10.96rem;
    margin:0 auto;
}
.list table tr td{
    font-size: 0.48rem;
    border:0.01rem solid #386998;
    border-radius: 0.2rem;
    -moz-border-radius: 0.2rem;
    -webkit-border-radius: 0.2rem;
    height: 1.5rem;
    width: 1.3rem;
    background: white;
    text-align: center;
    position: relative;
}
.alert{
    height: 1.08rem;
    line-height: 1.08rem;
    background: white;
    margin-top: 0.16rem;
    margin-bottom: 0.19rem;
    position: relative;
}
.alert span{
    margin-left: 0.45rem;
    font-size: 0.4rem;
}
.alert img{
    height: 0.67rem;
    width: 0.67rem;
    right: 0.52rem;
    top: 50%;
    margin-top: -0.335rem;
    position: absolute;
}

/*提示弹框*/
.alertBox{
    height: 100%;
    width: 100%;
    position: fixed;
    top: 0;
    left: 0;
    background: rgba(0,0,0,0.5);
    z-index: 1000;
}
.content{
    height: 7.96rem;
    width: 8.92rem;
    position: absolute;
    bottom: 0;
    top: 0;
    left: 0;
    right: 0;
    margin: auto;
    background: white;
    border-radius: 0.15rem;
}
.content .title{
    height: 1.3rem;
    background: #386998;
    position: relative;
    margin-top: -0.1rem;
    line-height: 1.3rem;
    border-top-left-radius: 0.15rem;
    border-top-right-radius: 0.15rem;
}
.content .title img:first-child{
    height: 0.67rem;
    height: 0.67rem;
    position: absolute;
    top: 50%;
    margin-top: -0.335rem;
    left: 0;
    margin-left: 0.4rem;
    margin-right: 0.3rem;
}
.content .title span{
    float: left;
    margin-left: 1.34rem;
    font-size: 0.5361rem;
}
.content .title img:nth-child(3){
    position: absolute;
    height: 0.47rem;
    width: 0.47rem;
    top: 50%;
    margin-top: -0.235rem;
    right: 0.47rem;
}
.info ul{
    padding: 0 0.14rem;
    margin-top: 0.5rem;
}
.info ul li{
    border-bottom: 0.02rem solid #a7a7a7;
    height: 1rem;
    line-height: 1rem;
    position: relative;
}
.info ul li:nth-child(3){
    border-bottom: 0.02rem solid #e7e7e7;
}
.info ul li:nth-child(3) label{
    margin-left: 0.78rem;
}
.info ul li:last-child{
    border: none;
}
.info ul li i:first-child{
    height: 0.14rem;
    width: 0.14rem;
    border-radius: 50%;
    display: inline-block;
    background: #29221a;
    -webkit-border-radius:50%;
    float: left;
    margin: 0.45rem 0.17rem 0 0.5rem
}
.info ul li .telInfo{
    font-size: 0.48rem;
}
.info ul li span{
    font-size: 0.48rem;
}
.info ul li .tel{
    width: 3.3rem;
    border: none;
}
.info ul li .check{
    height: 0.76rem;
    width: 0.76rem;
    position: absolute;
    right: 0.6rem;
    top: 50%;
    margin-top: -0.38rem;
    border-radius: 0.1rem;
    display: inline-block;
    background: url(../../images/border.png) no-repeat center; 
    background-size: 0.76rem 0.76rem;
}
.info ul li input:checked+i{
    background: url(../../images/checked.png) no-repeat center;
    background-size: 0.76rem 0.76rem;
}
.wei{
    width: 1.76rem;
    height: 0.57rem;
    border: 0.02rem solid #13bf89;
    color: #13bf89;
    line-height: 0.57rem;
    text-align: center;
    margin: auto 0.25rem; 
    border-radius: 0.1rem;
    display: inline-block;
    font-size: 0.4rem;
}
.duan{
    width: 1.76rem;
    height: 0.57rem;
    border: 0.02rem solid #386998;
    color: #386998;
    line-height: 0.57rem;
    text-align: center;
    border-radius: 0.1rem;
    display: inline-block;
    font-size: 0.4rem;
}


.upload{
    height: 1.08rem;
    line-height: 1.08rem;
    background: white;
    margin-top: 0.16rem;
    margin-bottom: 0.19rem;
    position: relative;
}
.text{
    margin-left: 0.45rem;
    font-size: 0.4rem;
    color: black;
}
.text span{
    font-size: 0.4rem;
    color: #386998;
}
.upload img{
    height: 0.68rem;
    width: 0.64rem;
    right: 0.52rem;
    top: 50%;
    margin-top: -0.34rem;
    position: absolute;
}
/*课程弹框*/
.courseBox{
    height: 100%;
    width: 100%;
    position: fixed;
    top: 0;
    left: 0;
    background: rgba(0,0,0,0.5);
    z-index: 1000;
}
.course{
    height: 10.2rem;
    width: 8.92rem;
    position: absolute;
    bottom: 0;
    top: 0;
    left: 0;
    right: 0;
    margin: auto;
    background: #eaeaea;
    border-radius: 0.15rem;
}
.course .title{
    height: 1.3rem;
    background: #386998;
    position: relative;
    margin-top: -0.1rem;
    line-height: 1.3rem;
    border-top-left-radius: 0.15rem;
    border-top-right-radius: 0.15rem;
}
.course .title img:first-child{
    height: 0.67rem;
    height: 0.67rem;
    position: absolute;
    top: 50%;
    margin-top: -0.335rem;
    left: 0;
    margin-left: 0.4rem;
    margin-right: 0.3rem;
}
.course .title span{
    float: left;
    margin-left: 1.34rem;
    font-size: 0.5361rem;
}
.course .title img:nth-child(3){
    position: absolute;
    height: 0.47rem;
    width: 0.47rem;
    top: 50%;
    margin-top: -0.235rem;
    right: 0.47rem;
}
.plan ul{
    padding: 0 0.14rem;
    margin-top: 0.16rem;
}
.plan ul li{
    background: white;
    border-radius: 0.2rem;
    margin-bottom: 0.17rem;
}
.plan ul li div{
    height: 1.16rem;
    line-height: 1.16rem;
}
.time{
    position: relative;
    color: #686868;
    border-bottom: 0.02rem solid #eaeaea;
}
.time i{
    height: 0.14rem;
    width: 0.14rem;
    border-radius: 50%;
    display: inline-block;
    background: #29221a;
    -webkit-border-radius:50%;
    float: left;
    margin: 0.45rem 0.17rem 0 0.5rem;
}
.time span{
    font-size: 0.48rem;
}
.time img{
    height: 0.69rem;
    width: 0.69rem;
    position: absolute;
    top: 50%;
    margin-top: -0.345rem;
    right: 0.4rem;
}
.name span{
    color: #386998;
    margin-left: 0.78rem;
}
.addClass{
    position: relative;
    background: #69dcb5;
    height: 1.04rem;
    line-height: 1.04rem;
    width: 8.66rem;
    margin: 0 auto;
    border-radius: 0.1rem;
}
.addClass img{
    width: 0.65rem;
    height: 0.65rem;
    position: absolute;
    top: 50%;
    margin-top: -0.325rem;
    margin-left: 2.65rem;
    margin-right: 0.24rem;
}
.addClass span{
    color: white;
    font-size: 0.48rem;
    margin-left: 3.6rem;
}

.tag{
    position: relative;
    height: 1.06rem;
    background: white;
}
.tag img{
    height: 1.06rem;
    width: 3.15rem;
    position: absolute;
    top: 0;
    left: 0;
}
.tag span{
    color: white;
    z-index: 100;
    position: absolute;
    left: 0.25rem;
    height: 1.06rem;
    margin-top: 0.2rem;
    font-size: 0.4804rem;
}
.type{
    padding-top: 0.42rem;
    background: white;
    padding-bottom: 0.66rem;
}
.type p span{
    float: left;
    font-size: 0.42rem;
}
.type p{
    clear: both;
    overflow: hidden;
}
.type p span:first-child{
    margin: 0 0.27rem 0.26rem 3.5rem;
}
.type p span:nth-child(2){
    margin-top: 0.05rem;
}
.red span strong{
    color: #db6066;
    font-size: 0.42rem;
    font-weight: normal;
}
.red span:first-child{
    height: 0.64rem;
    width: 0.64rem;
    background: #db6066;
    border-radius: 0.1rem;
    display: inline-block;
}
.blue span:first-child{
    height: 0.64rem;
    width: 0.64rem;
    background: #386998;
    border-radius: 0.1rem;
    display: inline-block;
}
.white span:first-child{
    height: 0.6rem;
    width: 0.6rem;
    background: #fff;
    border: 0.01rem solid #386998;
    border-radius: 0.1rem;
    display: inline-block;
}
.img{
    margin-top: 0.26rem;
}
.img img{
    width: 100%;
    height: 4.72rem;
}