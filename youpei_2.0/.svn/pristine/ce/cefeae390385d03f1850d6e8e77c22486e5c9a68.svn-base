/*公共样式 start*/
*{
	margin: 0;
	padding: 0;
	font-size: 0.12rem;
	}
li{
	list-style: none;
	}
a,button{
	text-decoration: none;
    border:none;
    -webkit-tap-highlight-color: rgba(0,0,0,0);
    -webkit-tap-highlight-color:transparent;
    outline:none;
    tap-highlight-color:rgba(0,0,0,0);
    tap-highlight-color:transparent;
	}
input,textarea,select,button{
	outline: none;
	}
img{
	border: none;
	display: block;
	}
textarea{
	resize: none;
	overflow: hidden;
	}
body,html{
	font-family: "微软雅黑";
    background-color: #f0f1f1;
}	
/*公共样式 end*/

/*定义字体*/
@font-face {
    font-family: "iconfont";
    src: url('fonts/iconfont.eot');
    src: url('fonts/iconfont.svg') format('svg'),
    url('fonts/iconfont.eot') format('embedded-opentype'),
    url('fonts/iconfont.ttf') format('truetype'),
    url('fonts/iconfont.woff') format('woff');
}
[data-icon]:before {
    content: attr(data-icon);
    display: inline-block;
    margin-right: .25rem;
     font-family: "iconfont" !important;
    font-size: 0.8rem;
    font-style: normal;
    -webkit-font-smoothing: antialiased;
    -webkit-text-stroke-width: .2px;
    -moz-osx-font-smoothing: grayscale;
    color: #386998!important;
}


/*头部样式*/
.header{
    height: 1.44rem;
    line-height: 1.44rem;
    background: white;
    overflow: hidden;
    position: fixed;
    z-index: 100000;
    top: 0;
    left: 0;
}
.header a{
    height: 1.44rem;
    float: left;
    margin: 0.28rem 0 0 0.35rem;
}
.header a img{
    height: 0.95rem;
    width: 2.4rem;

}
.search{
    float: left;
    position: relative;
    margin-left: 0.64rem;
}
.search input{
    width: 3.91rem;
    height: 0.88rem;
    border: 0.03rem solid #b7b7b7;
    border-radius: 0.1rem;
    line-height: 0.88rem;
    padding: 0 0.7rem 0 0.25rem;
    font-size: 0.36rem;
    color: #4e4e4e;
}
.area{
    float: right;
    /*margin-right: 0.54rem;*/
}
.area span{
    color: #386998;
    font-size: 0.42rem;
    float: left;
    margin-right: 1.3rem;
}
.area select{
    color: #386998;
    font-size: 0.42rem;
}
.area span img{
    height: 0.23rem;
    width: 0.37rem;
    float: left;
    margin-top: 0.61rem;
    margin-right: 0.24rem;
}
.search img{
    position: absolute;
    right: 0.1rem;
    top: 0.5rem;
    width: 0.53rem;
    height: 0.53rem;
}
/*中部样式*/
.middle{
    margin-top: 1.44rem;
    padding-bottom: 1.7rem;
}
/*轮播图*/
 .banner {
        clear: both;
        height: 3.75rem;
        overflow: hidden;
        position: relative;
    }

    .banner .bd {
        height: 3.75rem;
        overflow: hidden;
    }

    .banner .bd ul {
        overflow: hidden;
    }

    .banner .bd li {
        height: 3.75rem;
        overflow: hidden;
        float: left;
        width: 100%;
    }

    .banner .bd li img {
        height: 3.75rem;
        width: 100%;
        display: block;
    }

    .banner .hd {
        position: absolute;
        bottom: 0.32rem;
        text-align: center;
        width: 100%;
    }

    .banner .hd ul {
        display: inline-block;
        *display: inline;
        *zoom: 1;
    }

    .banner .hd ul li {
        width: 0.24rem;
        height: 0.24rem;
        text-align: center;
        display: inline-block;
        line-height: 0.24rem;
        border-radius: 50%;
        background-color: #fff;
        color: white;
        cursor: pointer;
        margin: 0 0.19rem;
        vertical-align: middle
    }

    .banner .hd ul li.on {
        background: #ffce55;
        font-weight: bold;
        color: white;;
    }
/*导航样式*/
.nav{
    background-color: white;
    overflow: hidden;
    margin-top: 0.12rem;
    margin-bottom: 0.18rem;
}
.nav ul{
    padding-top: 0.46rem;
    padding-bottom: 0.06rem;
}
.nav ul li{
    float: left;
    width: 20%;
    text-align: center;
    margin-bottom: 0.4rem;
}
.nav ul li img{
    margin: 0 auto;
    width: 1.12rem;
    height: 1.12rem;
    margin-bottom: 0.16rem;
}
.nav ul li span{
    color: #414141;
    font-size: 0.34rem;
}
/*限时抢购样式*/
.limit{
    background-color: white;
    margin-bottom: 0.2rem;
}
.limitHead{
    overflow: hidden;
    height: 1.03rem;
    border-bottom: 0.01rem solid #c1c1c1;
}
.limitHead .text{
    float: left;
    margin-top: 0.38rem;
    margin-left: 0.25rem;
}
.limitHead .text img{
    width: 0.48rem;
    height: 0.48rem;
    float: left;
}
.limitHead .text span{
    float: left;
    font-size: 0.42rem;
    color: #386998;
    font-weight: bold;
    margin-left: 0.21rem;
    margin-top: -0.05rem;
}
.limitHead .time{
    float: right;
    margin-top: 0.48rem;
    margin-right: 0.34rem;
}
.limitHead .time img{
    width: 0.29rem;
    height: 0.37rem;
    float: left;
}
.limitHead .time span{
    float: left;
    margin-left: 0.15rem;
    margin-top: -0.1rem;
    font-size: 0.36rem;
    color: #626262;
}
.limitContent{
    overflow: hidden;
    padding-bottom: 0.2rem;
}
.limitContent .img{
    position: relative;
    float: left;
    margin: 0.24rem 0.23rem 0.24rem 0.21rem;
}
.limitContent .img img{
    width: 3.85rem;
    height: 2.98rem;
}
.limitContent .tap{
    width: 0;
    height: 0;
    border-top: 1rem solid #db6066;
    border-left: 1rem solid transparent;
    position: absolute;
    right: 0rem;
    top: 0rem;
    border-bottom: 0.5rem solid transparent;
    border-right: 0.5rem solid #db6066;
}
.limitContent .tap span{
    position: absolute;
    right: -0.95rem;
    top: -0.6rem;
    font-size: 0.42rem;
    color: white;
    transform: rotate(45deg);
    font-weight: bold;
    width: 1.4rem;
}
.limitContent .text{
    float: left;
}
.limitContent .text h3{
    font-size: 0.48rem;
    color: #386998;
    line-height: 0.48rem;
    margin: 0.42rem 0 0.2rem;
}
.limitContent .type span{
    font-size: 0.34rem;
    color: white;
    margin-right: 0.2rem;
    padding: 0 0.14rem;
    height: 0.52rem;
    line-height: 0.52rem;
    display: inline-block;
    border-radius: 0.08rem;
}
.limitContent .type span:first-child{
    background-color: #f6d94f;
}
.limitContent .type span:nth-child(2){
    background-color: #83cf95;
}
.limitContent .type span:nth-child(2){
    background-color: #927cd6;
}
.limitContent .add{
    overflow: hidden;
    margin: 0.1rem 0 0.05rem;
}
.limitContent .add img{
    float: left;
    width: 0.29rem;
    height: 0.4rem;
    margin: 0.1rem 0.13rem 0 0;
}
.limitContent .add span{
    float: left;
    color: #9e9e9e;
    font-size: 0.36rem;
    margin-top: 0.05rem;
}
.limitContent .originalPrice{
    font-size: 0.36rem;
    color: #386998;
    margin-right: 0.6rem;
    text-decoration: line-through;
}
.limitContent .limitPrice{
    font-size: 0.4rem;
    font-weight: bold;
    color: #386998;
}
.limitContent .moneySign{
    color: #db6066;
}
.limitContent strong{
    font-size: 0.6rem;
     color: #db6066;
}
