    padding-top: 0.22rem;
}
.courseList .text .groupPrice{
    font-size: 0.4rem;
    color: #386998;
    font-weight: bold;
}
.courseList .text .moneySign{
    font-size: 0.4rem;
    color: #db6066;
}
.courseList .groupPrice strong{
    font-size: 0.6rem;
    color: #db6066;
}
.courseList .originalPrice{
    font-size: 0.36rem;
    color: #386998;
    text-decoration: line-through;
}
.courseList .text a{
    width: 2.15rem;
    height: 0.72rem;
    border-radius: 0.1rem;
    font-weight: bold;
    text-align: center;
    line-height: 0.72rem;
    font-size: 0.4rem;
    display: inline-block;
    margin-right: 0.3rem;
    border:0.05rem solid #db6066;
    color: #db6066;
    float: right;
    margin-top: 0.05rem;
    margin-bottom: 0.1rem;
}
.courseList .text .groupNum{
    clear: both;
}
.courseList .groupNum .finished{
    font-size: 0.2876rem;
    float: left;
}
.courseList .groupNum .bar{
    width: 1.9rem;
    height: 0.2rem;
    border-radius: 0.2rem;
    float: left;
    margin-top: 0.1rem;
    overflow: hidden;
    border:0.04rem solid #db6066;
    margin-left: 0.1rem;
}
.courseList .groupNum .bar span{
    height: 0.2rem;
    width: 1rem;
    background-color: #ffa3a7;
    border-radius: 0.2rem;
    display: block;
    margin-left: -0.05rem;
    float: left;
}
/*底部样式*/
.footer{
    clear: both;
    position: fixed;
    bottom: 0;
    width: 100%;
    z-index: 100000;
    left: 0
}
.icon{
    height: 1.5rem;
    background: #386998;
    height: 1.5rem;
    z-index: 100;
    opacity: 0.93;
}
.footer ul{
    display: flex;
    justify-content: space-around;
}
.footer ul li{
    height: 1.5rem;
    line-height: 1.6rem;
    position: relative;
}
.footer ul li>img{
    display: none;
}
.footer ul li >img{
    position: absolute;
    top: 0;
    right: 0;
    left: 0;
    margin: 0 auto;
    width: 0.42rem;
    height: 0.21rem;
    display: block;
}
.footer ul li a span{
    display: inline-block;
    font-size: 0.4rem;
    color: white;
    float: left;
}
.footer ul li a img{
    display: inline-block;
    height: 0.5rem;
    float: left;
    margin-top: 0.5rem;
    margin-right: 0.05rem;
}
.footer div{
    height: 0.2rem;
    background: #f0f1f1;
}

