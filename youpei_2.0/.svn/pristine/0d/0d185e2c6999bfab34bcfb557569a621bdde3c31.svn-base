    .empty{
        text-align: center;
        line-height: 1.3rem;
        font-size: 0.45rem;
        height: 1.3rem;
        background: white;
    }
    .Group .list-talent-product{
        margin-bottom: 9rem;
    }
    .Group .original{
        text-decoration: line-through;
        line-height: 4rem;
    }
    .Group .title font{
        line-height: 40px;
    }
    
body,html{
	background-color: #f0f1f1;
}
/*中部开始*/
.middle{
	margin-top: 2.8rem;
}

.advert{
	margin-bottom: 0.14rem;
}
.advert img{
    height: 4.5rem;
}
.productContent{
        overflow: hidden;
        margin-bottom: 1.5rem;
    }
    .productContent ul{
        overflow: hidden;
        margin-right: -2.6rem;
    }
    .productContent ul li{
        float: left;
        width: 5.6rem;
        background: white;
        margin-right: 0.25rem;
        margin-bottom: 0.25rem;
        position: relative;
    }
     .productContent .text{
        margin: 0.18rem 0 0 0.57rem;
    }

    .productContent ul li .img img{
        width: 100%;
        height: 4.78rem;
    }
    .productContent .address img{
        width: 0.33rem;
        height: 0.45rem;
        float: left;
        margin: 0 0.03rem;
    }
    .productContent .address span{
        float: left;
        font-size: 0.36rem;
        color: #757575;
        margin-top: -0.08rem;
    }
    .productContent .text{
        padding-bottom: 0.2rem;
    }
    .productContent .text h3{
        clear: both;
        color: #386998;
        font-size: 0.48rem;
        margin: 0.2rem 0;
    }
    .productContent .describe{
        font-size: 0.34rem;
        height: 1rem;
    }
    .productContent .type{
        overflow: hidden;
        margin-right: -0.15rem;
    }
    .productContent .type span{
        font-size: 0.34rem;
        height: 0.52rem;
        /*width: 1.4rem;*/
        padding:0 0.13rem;
        border-radius: 0.1rem;
        -webkit-border-radius: 0.1rem;
        /*text-align: center;*/
        line-height: 0.52rem;
        margin-right: 0.16rem;
        display: inline-block;
        color: white;
    }
    .productContent .type span:first-child{
        background: #f6d94f;
    }
    .productContent .type span:nth-child(2){
        background: #6fbee4;
    }
    .productContent .type span:nth-child(3){
        background: #de8cbd;
    }
    .productContent .original_price{
        color: #386998;
        text-decoration: line-through;
        margin-top: 0.1rem;
        float: left;
    }
    .productContent .text p{
        clear: both;
        line-height: 0.6rem;
        padding-bottom: 0.3rem;
    }
    .productContent .text p span{
        font-weight: bold;
    }
    .productContent .text p span:first-child{
        color: #386998;
        font-size: 0.35rem;
    }
    .productContent .text p span:nth-child(2){
        color: #db6066;
        font-size: 0.35rem;
    }
    .productContent .text p span:nth-child(3){
        color: #db6066;
        font-size: 0.5rem;
        margin-left: -0.1rem;
    }
    .productContent .text p span:nth-child(4){
        color: #999999;
        font-size: 0.3877rem;
    }
.light{
    height: 0.2rem;
    background: #f1f1f1;
    margin-bottom: -0.02rem;
}
.light2{
    height: 0.12rem;
    background: #f1f1f1;
}
.nav{
    position: fixed;
    margin-top: 1.2rem;
    width: 100%;
    top:0;
    z-index: 10000;
}
.smallNav{
     background:#fff;
    white-space: nowrap;
    overflow-y: hidden;
    overflow-x:scroll ;
    margin-top: -0.05rem;
}
.smallNav::-webkit-scrollbar {
    display: none;
}
.smallNav li{
    width: 24.5%;
    text-align: center;
    display: inline-block;
    height: 1.3rem;
    line-height: 1.3rem;
}

.smallNav li.on a{
    border: solid 0.03rem #f56b85;
    color: #f56b85;
}
.smallNav a{
    font-size: 0.45rem;
    /*display: block;*/
    color: #505050;
    padding: 0.1rem 0.5rem;
    border-radius: 0.2rem;
}