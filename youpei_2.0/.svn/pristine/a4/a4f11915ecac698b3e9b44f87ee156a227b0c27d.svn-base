html {
    height: 100%;
    overflow-x: hidden;
}

body {
    -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
    -webkit-text-size-adjust: none
}

.kap-wrap {
    -webkit-transition: all 0.2s cubic-bezier(0, 0, 0.25, 1);
    transition: all 0.2s cubic-bezier(0, 0, 0.25, 1);
}

.kap-page {
    position: absolute;
    left: 0rem;
    top: 0rem;
    width: 100%;
    min-height: 100%;
    display: none;
    -webkit-transition: all 0.2s cubic-bezier(0, 0, 0.25, 1);
    transition: all 0.2s cubic-bezier(0, 0, 0.25, 1);
}

/* .kap-page-default{} removed */
.kap-page-current {
    display: block;
}

.kap-view {
}

.kap-body-sider {
    width: 100%;
    height: auto;
    overflow-x: hidden;
    position: relative;
}

.kap-sider {
    position: fixed;
    left: 0rem;
    top: 0rem;
    width: 100%;
    height: 100%;
    z-index: 8999;
}

.kap-sider .kap-hitarea {
    background-color: rgba(0, 0, 0, 0);
    width: 100%;
    height: 100%;
    position: absolute;
    left: 0rem;
    top: 0rem;
    -webkit-transition: all 0.2s cubic-bezier(0, 0, 0.25, 1);
    transition: all 0.2s cubic-bezier(0, 0, 0.25, 1);
}

.kap-sider .kap-content {
    box-shadow: .3125rem 0rem .625rem rgba(0, 0, 0, 0.3);
    background-color: #fff;
    height: 100%;
    position: absolute;
    left: 0rem;
    top: 0rem;
    -webkit-transform: translateX(-100%);
    -webkit-transition: all 0.2s cubic-bezier(0, 0, 0.25, 1);
    transition: all 0.2s cubic-bezier(0, 0, 0.25, 1);
}

.kap-sider .kap-content-inset {
    box-shadow: none;
}

.kap-sider-right .kap-content {
    box-shadow: -.3125rem 0rem .625rem rgba(0, 0, 0, 0.3);
    -webkit-transform: translateX(100%);
    -ms-transform: translateX(100%);
    transform: translateX(100%);
}

.kap-sider-right .kap-content-inset {
    box-shadow: none;
}

.kap-body-sider .kap-sider .kap-content {
    -webkit-transform: translateX(0);
}

.kap-body-sider .kap-sider .kap-hitarea {
    background-color: rgba(0, 0, 0, 0.3);
}

.kap-body-sider .kap-sider .kap-hitarea-inset {
    background-color: rgba(0, 0, 0, 0);
}

.kap-mask {
    position: fixed;
    width: 100%;
    height: 100%;
    background-color: rgba(0, 0, 0, 0);
    z-index: 9000;
    -webkit-transition: all 0.2s cubic-bezier(0, 0, 0.25, 1);
    transition: all 0.2s cubic-bezier(0, 0, 0.25, 1);
}

.kap-mask-on {
    background-color: rgba(0, 0, 0, 0.3);
}
/**/
.kap-background{
    background-color: rgba(0, 0, 0, 0.3);
}
/**/

.kap-mask-blur {
    -moz-filter: blur(3px);
    -webkit-filter: blur(3px);
    -o-filter: blur(3px);
    -ms-filter: blur(3px);
    filter: blur(3px);
}

.kap-popwin {
    position: fixed;
    z-index: 90001;
    width: 100%;
    height: 100%;
    left: 0rem;
    top: 0rem;
}

.kap-popwin .kap-win {
    position: absolute;
    background-color: #fff;
    box-shadow: 0rem 0rem .5rem rgba(0, 0, 0, 0.5);
    color: #444;
    line-height: 2.5rem;
    left: 50%;
    top: 50%;
    font-size: .875rem;
    overflow: hidden;
    border-radius: .3125rem;
}

.kap-popwin .kap-title {
    padding: 0rem .625rem;
}

.kap-popwin .kap-hitarea {
    position: absolute;
    left: 0rem;
    top: 0rem;
    width: 100%;
    height: 100%;
    background-color: rgba(0, 0, 0, 0.1);
}

.kap-popwin .kap-content {
    line-height: 1.5rem;
    font-size: .875rem;
    padding: 0rem .625rem;
}

.kap-popwin .kap-btn-wrap {
    height: 3.125rem;
    line-height: 3.125rem;
    text-align: center;
    position: absolute;
    left: 0rem;
    bottom: 0rem;
    width: 100%;
    border-top: 1px solid #f0f0f0;
}

.kap-popwin a.kap-btn-submit, .kap-popwin a.kap-btn-submit:visited {
    height: 3.125rem;
    text-align: center;
    display: inline-block;
    text-decoration: none;
    color: inherit;
}

.kap-popwin a.kap-btn-cancel, .kap-popwin a.kap-btn-cancel:visited {
    height: 3.125rem;
    text-align: center;
    display: inline-block;
    text-decoration: none;
    color: inherit;
}

.kap-popwin a.kap-btn-submit:active, .kap-popwin a.kap-btn-cancel:active {
    background-color: #888;
    color: #fff;
}

.kap-tip {
    position: fixed;
    width: 90%;
    height: auto;
    top: 45%;
    z-index: 9000;
    text-align: center;
    left: 5%;
    opacity: 0;
    -webkit-transition: all 0.3s;
}

.kap-tip .kap-text {
    margin: 0 auto;
    display: inline-block;
    width: auto;
    padding: 2% 4%;
    background: rgba(0, 0, 0, 0.5);
    color: #fff;
    border-radius: .625rem;
}

.kap-tip.on {
    opacity: 1;
    top: 42%;
    -webkit-animation: kap_ani_fadein 0.3s 1 ease-in;
    animation: kap_ani_fadein 0.3s 1 ease-in;
}

.kap-alert .kap-content {
    line-height: 1.5rem;
    font-size: 1rem;
    padding: 1.875rem .625rem;
    text-align: center;
}

.kap-alert .kap-btn-cancel {
    width: 100%;
    height: 3.125rem;
    text-align: center;
    display: inline-block;
}

.kap-lockwin {
    position: fixed;
    z-index: 90002;
    width: 100%;
    height: 100%;
    left: 0rem;
    top: 0rem;
}

.kap-lockwin .kap-hitarea {
    position: absolute;
    left: 0rem;
    top: 0rem;
    width: 100%;
    height: 100%;
}

.kap-lockwin .kap-win {
    position: absolute;
    background-color: rgba(0, 0, 0, 0.7);
    box-shadow: 0rem 0rem .5rem rgba(0, 0, 0, 0.5);
    color: #FFF;
    line-height: 2.5rem;
    left: 0rem;
    bottom: 0rem;
    width: 100%;
}

.kap-lockwin .kap-content {
    line-height: 1.5rem;
    font-size: .875rem;
    padding: 0rem .625rem;
}

.kap-bottomwin {
    position: fixed;
    z-index: 90002;
    width: 100%;
    height: 100%;
    left: 0rem;
    top: 0rem;
}

.kap-bottomwin .kap-hitarea {
    position: absolute;
    left: 0rem;
    top: 0rem;
    width: 100%;
    height: 100%;
}

.kap-bottomwin .kap-win {
    position: absolute;
    background-color: #fff;
    box-shadow: 0rem -.3125rem .625rem rgba(0, 0, 0, 0.3);
    color: #FFF;
    line-height: 2.5rem;
    left: 0rem;
    bottom: 0rem;
    width: 100%;
}

.kap-bottomwin .kap-content {
    line-height: 1.5rem;
    font-size: .875rem;
}

