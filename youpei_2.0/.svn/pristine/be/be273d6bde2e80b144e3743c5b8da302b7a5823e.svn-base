                                // 遍历商品团购
                                if(count(data[i].groupProduct)){
                                    $(".groups"+n).append('<section class="shopGroups"><ul></ul></section>')
                                    var counts=count(data[i].groupProduct);
                                    if(counts%2!=0){
                                        counts=counts-1;
                                    }
