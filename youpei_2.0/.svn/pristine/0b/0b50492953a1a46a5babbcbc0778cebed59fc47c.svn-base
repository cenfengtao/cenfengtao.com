/*头部 start*/
.header{
    height: 1.2rem;
    line-height: 1.22rem;
    position: fixed;
    width: 100%;
    background: white;
    overflow: hidden;
    z-index: 100;
    clear: both;
}
.header a:first-child{
    left: 0.27rem;
    position: absolute;
}
.header a:first-child img{
    margin-top: 0.4rem;
    height: 0.5rem;
}
.header h3{
    text-align: center;
    font-size: 0.52rem;
    color: #065b9c;
    margin-left: 0.3rem;
    margin-top: 0.05rem;
}
/*头部 end*/
/*中部 start*/
.middle{
    padding-top: 1.4rem;
    background: #f0f1f1;
    padding-bottom: 1.5rem;
}
/*机构部分*/
.org{
	height: 3.8rem;
    margin-bottom: 0.2rem;
    background: white;
}
.org .left{
	width: 45%;
	float: left;
	margin-left: 5%;
	box-sizing:content-box;
}
.left img{
	height: 1.3rem;
	width: auto;
	margin-left: 5%;
	position: absolute;
	top: 0;
	bottom: 0;
	left: 0;
	right: 0;
	margin: auto;
}
.left .logo{
    height: 2rem;
    width: 100%;
    display: inline-block;
    position: relative;
}
.left>a{
    height: 0.7rem;
    width: 2.3rem;
    display: inline-block;
    font-size: 0.405rem;
    border-radius: 0.1rem;
    background: #386998;
    color: white;
    text-align: center;
    line-height: 0.7rem;
}
.left .seen{
    height: 0.7rem;
    width: 2.4rem;
    display: inline-block;
    font-size: 0.405rem;
    border-radius: 0.1rem;
    background: #db6066;
    color: white;
    text-align: center;
    line-height: 0.7rem;
    margin-left: 0.15rem;
}
.left p{
	text-align: center;
    font-size: 0.36rem;
    margin-top: 0.2rem;
}
.org .right{
	width: 50%;
	float: right;
}
.right ul{
	margin-top: 0.5rem;
}
.right li{
	height: 0.9rem;
	line-height: 0.9rem;
	margin-left: 0.5rem;
}
.right li span{
	font-size: 0.3647rem;
	float: left;
}
.right li span:nth-child(2){
	margin-top: 0.1rem;
	margin-left: 0.2rem;
}
.right li img{
	width: 0.52rem;
	height: 0.5rem;
	display: inline-block;;
}

.lunbo{
    height: 4.83rem;
    background: #386998;
    width: 100%;
    padding-top: 0.6rem;
}
.lunbo li{
    height: 4.2rem;
    width: 100%;
}
.lunbo li img{
    height: 4.2rem;
    width: 80%;
    margin: 0rem auto;
}
.banner {
        height: 4.2rem;
}

.banner .bd {
    height: 4.2rem;
    overflow: hidden;
}

.banner .bd li {
    height: 4.2rem;
    width: 100%;
}





.couponButton{
    height: 2rem;
    line-height: 2.3rem;
    text-align: center;
}
.couponButton span{
    display: inline-block;
    width: 90%;
    height: 1.15rem;
    line-height: 1.15rem;
    text-align: center;
    color: white;
    background: #db6066;
    font-size: 0.55rem;
    border-radius: 0.1rem;
}


/*精品课程*/
.course{
    overflow: hidden;
}
.course >h3{
    font-size: 0.2669rem;
    color: #959696;
    font-weight: normal;
    text-align: center;
    height: 1rem;
    line-height: 1rem;
}
.course >h3 strong{
    font-size: 0.5rem;
    color: #386998;
    font-weight: normal;
}
.course .advert img{
    width: 100%;
    height: 1.62rem;
}
.courseContent li{
    overflow: hidden;
    margin-bottom: 0.2rem;
    background: white;
    padding: 0.4rem 0;
}
.courseContent .img{
    position: relative;
    float: right;
    margin-right: 0.37rem;
}
.courseContent .img .courseImg{
    width: 5.39rem;
    height: 3.83rem;
}
.courseContent .tap{
    height: 1.04rem;
    width: 1.15rem;
    position: absolute;
    top: -0.05rem;
    left: 0.1rem;
}
.courseContent .logoTap{
    height: 1.04rem;
    width: 1.15rem;
    position: absolute;
}
.courseContent .img .imgLogo{
    width: 0.7rem;
    height: 0.7rem;
    position: absolute;
    top: 0.15rem;
    margin: 0 auto;
    left: 0;
    right: 0;
}
.courseContent .type{
    position: absolute;
    bottom: 0;
    left: 0;
    width: 100%;
    background: rgba(0,0,0,0.5);
    height: 0.8rem;
    line-height: 0.8rem;
    overflow: hidden;
}
.courseContent .type span{
    height: 0.56rem;
    line-height: 0.56rem;
    font-size: 0.34rem;
    padding: 0 0.15rem;
    color: white;
    display: inline-block;
    margin-right: 0.15rem;
    border-radius: 0.1rem;
}
.courseContent .type span:first-child{
    background-color: #ffc62d;
    margin-left: 0.3rem;
}
.courseContent .type span:nth-child(2){
    background-color: #e0656b;
}
.courseContent .type span:nth-child(3){
    background-color: #62b7e3;
}
.courseContent .text{
    float: left;
    margin-left: 0.24rem;
}
.courseContent .text h3{
    font-size: 0.48rem;
    line-height: 0.6rem;
    color: #386998;
    margin: 0.18rem 0rem;
    width: 5rem;
    height: 0.9rem;
}
.courseContent .text .groupPrice{
    font-size: 0.4rem;
    color: #386998;
    font-weight: bold;
}
.courseContent .text .moneySign{
    font-size: 0.4rem;
    color: #db6066;
}
.courseContent .groupPrice strong{
    font-size: 0.6rem;
    color: #db6066;
}
.courseContent .originalPrice{
    font-size: 0.36rem;
    color: #386998;
    text-decoration: line-through;
}
.courseContent .text .try{
    margin-top: 0.5rem;
}
.courseContent .text .try a{
    width: 2.15rem;
    height: 0.72rem;
    border-radius: 0.1rem;
    font-weight: bold;
    text-align: center;
    line-height: 0.72rem;
    font-size: 0.4rem;
    display: inline-block;
    margin-right: 0.3rem;
}
.courseContent .text .try a:first-child{
    border:0.05rem solid #83cf95;
    color: #83cf95;
}
.courseContent .text .try a:nth-child(2){
    border:0.05rem solid #db6066;
    color: #db6066;
}


.activity ul li{
    display: flex;
    flex-flow: row;
    background: white;
    height: 4.33rem;
    margin-bottom: 0.2rem;
}
.activity .img{
    width: 4.12rem;
    height: 3.37rem;
    position: relative;
    margin: 0.47rem 0.16rem 0 0.36rem;

}
.activity .img img{
    width: 100%;
    height: 100%;
}
.activity .img .type{
    height: 0.6rem;
    line-height: 0.6rem;
    background: rgba(0,0,0,0.3);
    width: 100%;
    position: absolute;
    bottom: 0;
    left: 0;
}
.activity .img .type span{
    height: 0.43rem;
    line-height: 0.43rem;
    font-size: 0.2593rem;
    padding: 0 0.1rem;
    color: white;
    display: inline-block;
    margin-right: 0.13rem;
    border-radius: 0.1rem;
}
.activity .img .type span:first-child{
    background-color: #ffc62d;
    margin-left: 0.23rem;
}
.activity .img .type span:nth-child(2){
    background-color: #e0656b;
}
.activity .img .type span:nth-child(3){
    background-color: #62b7e3;
    margin-right: 0;
}
.activity .text{
    flex: 3;
    margin-top: 0.47rem;
}
.activity .text h3{
    font-size: 0.42rem;
    line-height: 0.73rem;
    height: 0.73rem;
    color: #386998;
    width: 95%;
    padding-left: 0;
}
.activity .text .work{
    height: 0.86rem;
    line-height: 0.86rem;
    overflow: hidden;
    width: 95%;
}
.activity .text .workNum{
    float: left;
}
.activity .workNum span{
    font-size: 0.4rem;
}
.activity .text .workNum strong{
    font-weight: normal;
    color: #386998;
    font-size: 0.4rem;
}
.activity .text .heat{
    float: left;
    font-size: 0.4rem;
    margin-left: 0.3rem;
}
.activity .text .heat span{
    font-size: 0.4rem;
    float: left;
    margin-right: 0.1rem;
}
.activity .text .heat img{
    width: 0.4rem;
    height: 0.38rem;
    display: inline-block;
    margin-top: 0.22rem;
}
.activity .text .date span{
    font-size: 0.4rem;
    height: 0.86rem;
    line-height: 0.86rem;
}
.activity .text .date .dateTime{
    color: #386998;
}
.activity .text>a{
    float: right;
    height: 0.8rem;
    width: 3rem;
    text-align: center;
    line-height: 0.8rem;
    background: #db6066;
    color: white;
    font-size: 0.4rem;
    display: inline-block;
    margin-right: 0.4rem;
    border-radius: 0.1rem;
}


/*视频部分*/
.video{
    height: 6.34rem;
    position: relative;
    margin-bottom: 0.2rem;
}
.video img{
    height: 6.34rem;
    width: 100%
}
.video .playVideo{
    height: 2.03rem;
    width: 2.03rem;
    position: absolute;
    top: 0;
    bottom: 0；;
    bottom: 0;
    left: 0;
    right: 0;
    margin: auto
}
.video .desc{
    width: 2.44rem;
    height: 0.92rem;
    background: #ffcd00;
    border-top-right-radius: 0.46rem;
    border-bottom-right-radius: 0.46rem;
    line-height: 0.92rem;
    position: absolute;
    top: 0;
    left: 0;
    text-align: center;
}
.video .desc span{
    font-size: 0.36rem;
    color: white;
    /*margin-left: 0.36rem;*/
}


.tellBoxs{
    position: fixed;
    left: 0;
    top: 0;
    background: rgba(0,0,0,0.5);
    width: 100%;
    height: 100%;
    z-index: 1000000000;
    display: none;
}
 .tellContents{
    border-radius: 0.1rem;
    position: absolute;
    top: 4rem;
/*    bottom: 0;*/
    left: 0;
    right: 0;
    /*height: 4.9rem;*/
    width:8.92rem;
    margin: auto;
    background: white;
}
.tellContents .closeBtn{
    float: right;
    height: 0.47rem;
    width: 0.47rem;
    margin-top: 0.28rem;
    margin-right: 0.3rem;
}
.tellContents .title{
    height: 1.3rem;
    background: #386998;
    position: relative;
    margin-top: -0.1rem;
    line-height: 1.3rem;
    border-top-left-radius: 0.15rem;
    border-top-right-radius: 0.15rem;
}
.tellContents .title img:first-child{
    height: 0.67rem;
    height: 0.67rem;
    position: absolute;
    top: 50%;
    margin-top: -0.335rem;
    left: 0;
    margin-left: 0.4rem;
    margin-right: 0.3rem;
}
.tellContents .title span{
    float: left;
    margin-left: 1.34rem;
    font-size: 0.5361rem;
    color: white;
}
.tellContents .title img:nth-child(3){
    position: absolute;
    height: 0.47rem;
    width: 0.47rem;
    top: 50%;
    margin-top: -0.235rem;
    right: 0.47rem;
}


.coupons{
    background: white;
}
.coupons ul{
    padding-top: 0.4rem;
    /*margin-bottom: 0.2rem;*/
    padding-bottom: 0.4rem;
    background: #f1f1f1;
}
.coupons ul li{
    position: relative;
}
.coupons ul li .get{
    position: absolute;
    right: 0;
    top: 0;
    display: inline-block;
    width: 2.2rem;
    height: 2.44rem;
    text-align: center;
    margin-top: 0.4rem;
    font-size: 0.6rem;
}
.coupons ul li .red{
    color: #dc4e5a;
}
.coupons ul li .orange{
    color: #ed9c00;
}
.coupons ul .cash_coupon{
    width: 8.43rem;
    margin: 0 auto 0.4rem;
    height: 2.44rem;
    background: url(../../../../Public/images/orgCoupn.png) no-repeat;
    background-size: 8.43rem 2.44rem;
}
.coupons ul .coupon{
    width: 8.43rem;
    margin: 0 auto 0.4rem;
    height: 2.44rem;
    background: url(../../../../Public/images/orgCashCoupn.png) no-repeat;
    background-size: 8.43rem 2.44rem;
}
.texts{
    overflow: hidden;
    width: 77%;
}
.texts >span{
    font-size: 0.5407rem;
    float: left;
    color: white;
    margin-top: 0.4rem;
    width: 2.5rem;
    text-align: center;
}
.texts span strong{
    font-size: 0.8rem;
}
.cash_coupon .texts div{
    float: right;
    margin: 0.2rem 0.1rem 0 0.18rem;
}
.cash_coupon .texts div span{
    color: white;
    float: left;
    font-size: 0.5rem;
    margin-bottom: 0.1rem;
}
.cash_coupon .texts div p{
    color: white;
    float: left;
    clear: both;
    font-size: 0.2426rem;
    height: 0.8rem;
    width: 3.5rem;
}
.coupon .texts div{
    float: right;
    margin: 0.2rem 0.1rem 0 0.18rem;
}
.coupon .texts div span{
    color: white;
    float: left;
    font-size: 0.5rem;
    margin-bottom: 0.1rem;
}
.coupon .texts div p{
    color: white;
    float: left;
    clear: both;
    font-size: 0.2426rem;
    width: 3.5rem;
    height: 0.8rem;
}
.hr{
    width: 64%;
    height: 0.03rem;
    background: whtie;
    background: white;
    margin-left: 0.3rem;
    margin-top: 0.1rem;
    margin-bottom: 0.1rem;
}
.content >p{
    clear: both;
    font-size: 0.2704rem;
    color: white;
    width: 66%;
    margin-left: 0.3rem;
}
.coupons .img{
    position: absolute;
    top: -0.08rem;
    right: 3.4rem;
    height: 1.04rem;
    width: 1.15rem;

}
.coupons .imgTap{
    height: 1.04rem;
    width: 1.15rem;
}
.coupons .imgLogo{
    height: 0.69rem;
    width: 0.7rem;
    margin: auto;
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
}