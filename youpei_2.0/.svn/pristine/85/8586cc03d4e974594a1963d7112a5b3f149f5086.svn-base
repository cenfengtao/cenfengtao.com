<script>
    var windowWidth=$(window).width();
    if(windowWidth<680){
        $(".swiper-container").css("height","7.8rem")
        $(".swiper-slide .rig").css("height","6.375rem")
        $(".swiper-slide .rig .price .original").css("margin-top","0")
        $(".productContent ul li .img img").css("height","6.3rem")
        $("html").css("font-size","26px")
    }
     var mySwiper1 = new Swiper('.swiper-container1',{
        pagination: '.pagination1',
        loop:true,
        grabCursor: true,
        paginationClickable: true,
        direction: 'vertical',
    });
    $('.arrow-left1').on('click', function(e){
        e.preventDefault();
        mySwiper1.swipePrev();
    });
    $('.arrow-right1').on('click', function(e){
        e.preventDefault();
        mySwiper1.swipeNext();
    });
    function shopDetail(){
        window.location.href="/index.php/Boutique/index.html"
    }
    function tuanDetail(){
        window.location.href="/index.php/Groups/index.html"
    }
</script>
