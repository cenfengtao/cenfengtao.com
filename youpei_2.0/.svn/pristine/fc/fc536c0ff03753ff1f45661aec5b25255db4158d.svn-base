body,html{
	background: #f1f1f1;
}

/*中部*/
.product{
	background: white;
	overflow: hidden;
	margin-bottom: 0.22rem;
}
.img{
	float: left;
	margin:0.51rem 0.34rem;
}
.img img{
	width: 5.54rem;
	height: 3.8rem;
}
.middle{
	margin-top: 1.3rem;
}
.content{
	float: left;
	position: relative;
	height: 4.9rem;
	width: 4.95rem;
}
.content h3{
	color: #386998;
	font-size: 0.6rem;
	margin-top: 0.8rem;
}
.content .original{
	position: absolute;
	left: 0;
	bottom: 1.7rem;
	font-size: 0.42rem;
	color: #4b4b4b;
	text-decoration: line-through;
}
.content .now{
	position: absolute;
	left: 0;
	bottom: 0.9rem;
}
.now span:first-child{
	color: #386998;
	font-size: 0.48rem;
	font-weight: bold;
}
.now span:nth-child(2){
	font-size: 0.4rem;
	color: #db6066;
}
.now span:nth-child(3){
	font-size: 0.6rem;
	color: #db6066;
	font-weight: bold; 
}
.now span:nth-child(4){
	font-size: 0.4rem;
	color: #386998;
	font-weight: bold;
}




.way{
	background: white;
	margin-bottom: 0.22rem;
}
.tag{
	position: relative;
}
.tag img{
	height: 0.81rem;
	width: 3.06rem;
}
.tag span{
	color: white;
	position: absolute;
	left: 0.4rem;
	top: 0.12rem;
	font-size: 0.4804rem;
}
.rule{
	margin-left: 0.4rem;
	margin-top: 0.4rem;
	/*padding-bottom: 1.1rem;*/
}
.rule h3{
	font-size: 0.45rem;
}
.rule p{
	font-size: 0.42rem;
	color: #585858;
	line-height: 0.68rem;
}
/*.rule p:nth-child(5){
	color: #e95e5e;
}*/
.rule .explain{
	color: #e95e5e;
	font-size: 0.45rem;
}
.serve div{
	width: 4.2rem;
	margin: 0 auto;
}
.serve img{
	width: 2rem;
	height: 2rem;
	padding-bottom: 0.2rem;
	display: inline-block;
}

.rate{
	background: white;
	padding-bottom: 0.74rem;
	margin-bottom: 0.22rem;
}
.rate .title{
	position: relative;
}
.rate .title h3{
	width: 100%;
	text-align: center;
	height: 1.37rem;
	line-height: 1.37rem;
	font-size: 0.52rem;
	color: #386998;
}
.rate .title span{
	position: absolute;
	border: 0.04rem solid #386998;
	width: 1.14rem;
	left: 3.24rem;
	top: 0.67rem;
}
.rate .title span:nth-child(3){
	left: 7rem;
}
.light{
	position: relative;
	width: 10.6rem;
	height: 0.87rem;
	border: 0.06rem solid #386998;
	margin-left: 0.35rem;
	border-radius: 0.48rem;
	-webkit-border-radius: 0.48rem;
	overflow: hidden;
	z-index:1;
}
.green{
	border-right: 0.06rem solid #386998;
	height: 0.87rem;
	position: absolute;
	background: #71ccb0;
	width: 9rem;
	border-radius: 0.48rem;
	-webkit-border-radius: 0.48rem;
	z-index: 100;
	text-align: center;
	color: black;
	line-height: 0.9rem;
	font-size: 0.48rem;
}
.gray{
	height: 0.87rem;
	position: absolute;
	background: gray;
	width: 10.7rem;
	-webkit-border-radius: 0.48rem;
	border-radius: 0.48rem;
	z-index: 100;
	line-height: 0.9rem;
}
.gray span{
	float: right;
	color: white;
	padding-right: 0.5rem;
	font-size: 0.48rem;
}
.userImg{
	background: white;
	overflow: hidden;
}
.userImg li{
	float: left;
	width: 12.5%;
}
.userImg li img{
	width: 1rem;
	height: 1rem;
	margin: 0 auto;
	border-radius: 50%;
    border: 0.01rem solid lightblue;
}

.alert{
	background: white;
	height: 0.95rem;
	line-height: 0.95rem;
}
.alert img{
	width: 0.56rem;
	height: 0.56rem;
	float: left;
	margin-left: 0.38rem;
	margin-top: 0.22rem;
	margin-right: 0.2rem;
}
.alert span{
	font-size: 0.42rem;
}
.alert span strong{
	color: #e49195; 
	text-decoration: underline;
	font-size: 0.7rem;
	font-weight: bold;
}
.alert a{
	font-size: 0.42rem;
	font-weight: bold;
	text-decoration: underline;
	color: #436e98;
	margin-left: 0.35rem;
}
.much{
	margin-left: 0rem!important;
	font-size: 0.7rem!important;
}
.buy{
	margin-bottom: 1rem;
}
.buy a{
	width: 7.11rem;
	height: 1.41rem;
	text-align: center;
	line-height: 1.41rem;
	background: #db6066;
	font-size: 0.63rem;
	color: white;
	margin: 1.8rem auto;
	margin-top: 0.8rem;
	display: block;
	border-radius: 0.1rem;
	-webkit-border-radius:0.1rem;
}
.together{
	padding-bottom: 1rem;
	position: relative;
}
.together a:first-child{
	width: 7.11rem;
	height: 1.41rem;
	text-align: center;
	line-height: 1.41rem;
	background: #db6066;
	font-size: 0.63rem;
	color: white;
	margin: 0.7rem auto;
	display: block;
	border-radius: 0.1rem;
	-webkit-border-radius:0.1rem;
}
.together a:nth-child(2){
	width: 7.11rem;
	height: 1.41rem;
	text-align: center;
	line-height: 1.41rem;
	background: #f4b014;
	font-size: 0.63rem;
	color: white;
	margin: 0 auto 1.5rem;
	display: block;
	border-radius: 0.1rem;
	-webkit-border-radius:0.1rem;
}
.together img{
	width: 2.06rem;
	height: 1.91rem;
	position: absolute;
	top: -0.25rem;
	right: 1.7rem;
}
.box{
	position: fixed;
	z-index: 10000;
	height: 100%;
	width: 100%;
	background: rgba(0,0,0,0.5);
	top: 0;
	left: 0;
}
.box .content{
	height: 9.96rem;
	width: 8.92rem;
	border-radius: 0.1rem;
	-webkit-border-radius:0.1rem;
	position: fixed;
	top:0;
	bottom: 0;
	left: 0;
	right: 0;
	margin: auto;
	background: white;
}
.box .content .top{
	height: 1.3rem;
	width: 100%;
	background: #386998;
	line-height: 1.3rem;
	border-top-left-radius: 0.1rem;
	border-top-right-radius: 0.1rem;
}
.top{
	overflow: hidden;
}
.top img:first-child{
	height: 0.65rem;
	width: 0.64rem;
	float: left;
	margin-left: 0.35rem;
	margin-top: 0.32rem;
	margin-right: 0.3rem;
}
.top img:nth-child(3){
	float: right;
	height: 0.56rem;
	width: 0.43rem;
	margin-top: 0.32rem;
	margin-right: 0.35rem;
}
.box .content .top span{
	font-size: 0.5361rem;
	color: white;
}
.box .content .reduce p{

	width: 5rem;
	margin: 0 auto;
	text-align: center;
	font-size: 0.6rem;
	margin-top: 1rem;
	margin-bottom: 0.5rem;
}
.box .content .reduce img{
	height: 2rem;
	width: 2rem;
	margin: 0 auto 0.5rem;
}
.reduce p span{
	font-size: 0.6rem;
	color: #809ab7;
}
.box .content >a{
	height: 1.19rem;
	width: 5.09rem;
	text-align: center;
	line-height: 1.19rem;
	display: block;
	color: white;
	background: #db6066;
	font-size: 0.5929rem;
	margin: 0 auto;
	border-radius: 0.15rem;
}
.share{
	position: fixed;
	z-index: 10000;
	height: 100%;
	width: 100%;
	background: rgba(0,0,0,0.7);
	top: 0;
	left: 0;
}
.share >span,.morePeople >span{
	position: absolute;
	right: 0rem;
	top: 1.11rem;
	font-size: 2rem;
	color: white;
	/*transform: rotate(45deg);*/
}
.share .step{
	color: white;
	height: 6rem;
	position: absolute;
	top: 0;
	bottom: 0;
	left: 0;
	right: 0;
	margin: auto;
	width: 80%;
}
.step p{
	margin-bottom: 0.3rem;
	font-size: 0.5rem;
	line-height: 0.8rem;
}
.step p:first-child{
	font-size: 0.6rem;
}
.morePeople{
	position: fixed;
	z-index: 10000;
	height: 100%;
	width: 100%;
	background: rgba(0,0,0,0.7);
	top: 0;
	left: 0;
}
.morePeople p{
	text-align: center;
	color: white;
	height: 3rem;
	position: absolute;
	top: 0;
	bottom: 0;
	left: 0;
	right: 0;
	margin: auto;
	width: 80%;
	font-size: 0.5rem;
}
.morePeople img{
	height: 6rem;
	width: 8rem;
}
.type{
	position: fixed;
	z-index: 10000;
	height: 100%;
	width: 100%;
	background: rgba(0,0,0,0.2);
	top: 0;
	left: 0;
}
.type div{
	color: black;
	top: 0;
	bottom: 0;
	left: 0;
	right: 0;
	margin: auto;
	width: 60%;
	position: absolute; 
	height: 5rem;
	text-align: center;
	background: #f1f1f1;
	border-radius: 0.2rem;
}
.type div p{
	font-size: 0.3rem;
	width: 80%;
	margin:auto;
	height: 2rem;
	top: 0;
	bottom: 0;
	left: 0;
	right: 0;
	position: absolute;
	text-align: center;
	color: #4561c9;
}
.detailsText{
	margin-top: -0.35rem;
	padding:0.6rem;
}
.detailsText img{
	display: block;
    width: 100% !important;
    height: auto !important;
    box-shadow: 0.0625rem 0.0625rem 0.5rem rgba(0, 0, 0, 0.3);
    /* background: url(../images/icon_img.png) center 50% no-repeat #f0f1f2; */
    background-size: 4.6875rem auto;
    max-width: 100% !important;
}
.tag{
	background: white;
	margin-top: 0.2rem;
}
.detailsText p {
    padding: .5rem;
    text-align: justify;
    font-size: 0.4rem;
}