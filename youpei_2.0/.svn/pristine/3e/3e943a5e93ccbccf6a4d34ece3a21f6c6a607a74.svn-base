/*砍价图标样式*/
.bargain{
    position: absolute;
    bottom: 0rem;
    left: 0;
    z-index: 100;
}
.bargainText{
    width: 7rem!important;
    height: auto!important;
    margin-top: 1rem;
}
.courseBargain1{
    margin-left: 0.4rem;
    width: 2rem;
    margin-top: 0.8rem;
}
.courseBargain2{
    margin-left: 0.4rem;
    width: 1.3rem;
    position: absolute;
    top: 0.2rem;
    left: 0.3rem;
}
.bargainImg{
    width: 5rem!important;
    height: auto!important;
    position: absolute;
    top: -1rem;
    left: 1rem;
