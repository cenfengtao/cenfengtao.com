     /*轮播 start*/
    .banner {
        clear: both;
        height: 8.5rem;
        overflow: hidden;
        position: relative;
        margin-top: 2.5rem;
    }

    .banner .bd {
        height: 8.5rem;
        overflow: hidden;
    }

    .banner .bd ul {
        overflow: hidden;
    }

    .banner .bd li {
        height: 8.5rem;
        overflow: hidden;
        float: left;
        width: 100%;
    }

    .banner .bd li img {
        height: 8.5rem;
        width: 100%;
        display: block;
    }

    .banner .hd {
        position: absolute;
        bottom: 0.3rem;
        text-align: center;
        width: 100%;
    }

    .banner .hd ul {
        display: inline-block;
        *display: inline;
        *zoom: 1;
    }

    .banner .hd ul li {
        width: .5rem;
        height: .5rem;
        text-align: center;
        display: inline-block;
        line-height: 24px;
        border-radius: 12px;
        background-color: #fff;
        color: white;
        cursor: pointer;
        margin: 0 .25rem;
        vertical-align: middle
    }

    .banner .hd ul li.on {
        background: #ffce55;
        font-weight: bold;
        color: white;;
    }
    .nowPrice{
        color: #386998;
        font-size: 1rem;
        font-weight: bold;
        position: absolute;
        /*left: 10rem;*/
        right: 0;
        bottom: 0;
    }
    .originalPrice{
        font-size: 1rem;
        line-height: 1.5rem;
        font-weight: bold;
        position: absolute;
        /*left: 10rem;*/
        right: 0;
        text-decoration: line-through;
        bottom: 0rem;
        color: white;
        background: rgba(0,0,0,0.2);
    }
    .list-card .price{
        float: none!important;
        font-size: 1.5rem!important;
    }
    .list-card .price:before {
        content: none!important;
    }
/*    .title{
        color: #2ab4e2!important;
    }*/
    .current strong{
        color: #386998;
    }
    .current b{
        font-size: 45px;
    }
    .current .start{
        font-size: 30px;
        color: gray;
    }
    .bor:before{
        background: #386998!important;
    }
    .device{
        border-radius: 10px;
        -webkit-border-radius:10px;
    }
