body,html{
	background-color: #f0f1f1;
}
/*中部开始*/
.middle{
	margin-top: 1.34rem;
}

/*轮播 start*/
    .banner {
        clear: both;
        height: 3.75rem;
        overflow: hidden;
        position: relative;
    }

    .banner .bd {
        height: 3.75rem;
        overflow: hidden;
    }

    .banner .bd ul {
        overflow: hidden;
    }

    .banner .bd li {
        height: 3.75rem;
        overflow: hidden;
        float: left;
        width: 100%;
    }

    .banner .bd li img {
        height: 3.75rem;
        width: 100%;
        display: block;
    }

    .banner .hd {
        position: absolute;
        bottom: 0.32rem;
        text-align: center;
        width: 100%;
    }

    .banner .hd ul {
        display: inline-block;
        *display: inline;
        *zoom: 1;
    }

    .banner .hd ul li {
        width: 0.24rem;
        height: 0.24rem;
        text-align: center;
        display: inline-block;
        line-height: 0.24rem;
        border-radius: 50%;
        background-color: #fff;
        color: white;
        cursor: pointer;
        margin: 0 0.19rem;
        vertical-align: middle
    }

    .banner .hd ul li.on {
        background: #db6066;
        font-weight: bold;
        color: white;;
    }


    /*课程*/
    .courseTitle,.productTitle,.articleTitle{
    	height: 1.12rem;
    	line-height: 1.12rem;
    	background-color: white;
    	overflow: hidden;
    }
    .courseTitle img,.productTitle img,.articleTitle img{
    	height: 0.72rem;
    	width: 0.75rem;
    	margin: 0.24rem 0.27rem 0 0.55rem;
    	float: left;
    }
    .courseTitle span,.productTitle span,.articleTitle span{
    	float: left;
    	font-size: 0.48rem;
    	color: #386998;
    	margin-top: 0.08rem;
    	font-weight: bold;
    }
    .courseTitle a,.productTitle a,.articleTitle a{
    	color: #757575;
    	font-size: 0.45rem;
    	float: right;
    	margin-right: 0.4rem;
    }


	.courseContent,.productContent,.articleContent{
		border-top: 0.02rem solid #f1f1f1;
	}
	.courseContent ul li{
		background: white;
		margin-bottom: 0.15rem;
		height: 3rem;
        position: relative;
	}
	.courseImg{
		float: left;
		margin: 0.3rem 0 0 0.38rem;
	}
	.courseImg a{
		position: relative;
		display: block;
	}
	.courseImg img{
		height: 2.4rem;
		width: 3.1rem;
	}
	.courseImg a span{
		position: absolute;
		top: 0;
		left: 0;
		display: block;
		height: 0.55rem;
		line-height: 0.55rem;
		text-align: center;
		font-size: 0.34rem;
		color: white;
		padding: 0 0.08rem;
		background: #f7b400;
	}
	.courseText{
		float: left;
		margin-left: 0.34rem;
        position: relative;
	}
	.courseText h3{
		color: #386998;
		font-size: 0.6rem;
		margin-top: 0.39rem;
		line-height: 0.2295rem;
        margin-bottom: 0.16rem;
	}
    .courseText .price span:first-child{
        font-size: 0.36rem;
        color: #386998;
        text-decoration: line-through;
        margin-right: 0.3rem;
    }
    .courseText .price span:nth-child(2){
        font-size: 0.4rem;
        color: #386998;
        font-weight: bold;
    }
    .courseText .price span:nth-child(2) strong{
        font-weight: bold;
        color: #db6066;
        font-size: 0.6rem;
    }
    .courseText .join{
        font-size: 0.3rem;
        color: #9e9e9e;

    }
    .courseText .join strong{
        color: #386998;
    }
    .courseText .date{
        font-size: 0.3rem;
        color: #9e9e9e;
    }
    .courseText .go{
        position: absolute;
        right: 0;
        bottom: 0;
        font-size: 0.4rem;
        color: white;
        background: #db6066;
        height: 0.7rem;
        line-height: 0.7rem;
        width: 2.16rem;
        text-align: center;
        border-radius: 0.1rem;
        -webkit-border-radius:0.1rem;
        margin-bottom: -0.15rem;
    }

    .courseContent .text{
        float: left;
        margin-left: 0.1rem;
        width: 7.5rem;
        position: relative;
    }

    .courseContent ul li .img img{
        width: 100%;
        height: 2.78rem;
    }
    .courseContent .address{
        margin-top: 0.2rem;
        float: left;
    }
    .courseContent .address img{
        width: 0.33rem;
        height: 0.45rem;
        float: left;
        margin: 0 0.03rem;
    }
    .courseContent .address span{
        float: left;
        font-size: 0.36rem;
        color: #757575;
        margin-top: -0.08rem;
    }
    .courseContent .text h3{
        clear: both;
        color: #386998;
        font-size: 0.5rem;
        margin: 0.2rem 0;
        line-height: 0.6rem;
    }
    .courseContent .type{
        overflow: hidden;
        margin-right: -0.15rem;
    }
    .courseContent .type span{
        float: left;
        font-size: 0.34rem;
        height: 0.52rem;
        /*width: 1.4rem;*/
        padding:0 0.13rem;
        border-radius: 0.1rem;
        -webkit-border-radius: 0.1rem;
        /*text-align: center;*/
        line-height: 0.52rem;
        margin-right: 0.16rem;
        display: inline-block;
        color: white;
    }
    .courseContent .type span:first-child{
        background: #f6d94f;
    }
    .courseContent .type span:nth-child(2){
        background: #6fbee4;
    }
    .courseContent .type span:nth-child(3){
        background: #de8cbd;
    }
    .courseContent .original_price{
        color: #386998;
        text-decoration: line-through;
        margin-top: 0.1rem;
        float: left;
        clear: both;
        font-size: 0.3rem;
    }
    .courseContent .text p{
        /*clear: both;*/
        float: left;
        position: absolute;
        right: 0;
        bottom: 0;
        line-height: 0.6rem;
        margin-left: 0.25rem;
    }
    .courseContent .text p span{
        font-weight: bold;
    }
    .courseContent .text p span:first-child{
        color: #386998;
        font-size: 0.35rem;
    }
    .courseContent .text p span:nth-child(2){
        color: #db6066;
        font-size: 0.35rem;
    }
    .courseContent .text p span:nth-child(3){
        color: #db6066;
        font-size: 0.5rem;
        margin-left: -0.1rem;
    }
    .courseContent .text p span:nth-child(4){
        color: #999999;
        font-size: 0.35rem;
    }
    .productContent{
        overflow: hidden;
        margin-bottom: 1.5rem;
    }
    .productContent ul{
        overflow: hidden;
        margin-right: -2.6rem;
    }
    .productContent ul li{
        float: left;
        width: 5.6rem;
        background: white;
        margin-right: 0.25rem;
        margin-bottom: 0.25rem;
        position: relative;
    }
     .productContent .text{
        margin: 0.18rem 0 0 0.57rem;
    }

    .productContent ul li .img img{
        width: 100%;
        height: 4.78rem;
    }
    .productContent .address img{
        width: 0.33rem;
        height: 0.45rem;
        float: left;
        margin: 0 0.03rem;
    }
    .productContent .address span{
        float: left;
        font-size: 0.36rem;
        color: #757575;
        margin-top: -0.08rem;
    }
    .productContent .text{
        padding-bottom: 0.2rem;
    }
    .productContent .text h3{
        clear: both;
        color: #386998;
        font-size: 0.48rem;
        margin: 0.2rem 0;
    }
    .productContent .describe{
        font-size: 0.34rem;
    }
    .productContent .type{
        overflow: hidden;
        margin-right: -0.15rem;
    }
    .productContent .type span{
        font-size: 0.34rem;
        height: 0.52rem;
        /*width: 1.4rem;*/
        padding:0 0.13rem;
        border-radius: 0.1rem;
        -webkit-border-radius: 0.1rem;
        /*text-align: center;*/
        line-height: 0.52rem;
        margin-right: 0.16rem;
        display: inline-block;
        color: white;
    }
    .productContent .type span:first-child{
        background: #f6d94f;
    }
    .productContent .type span:nth-child(2){
        background: #6fbee4;
    }
    .productContent .type span:nth-child(3){
        background: #de8cbd;
    }
    .productContent .original_price{
        color: #386998;
        text-decoration: line-through;
        margin-top: 0.1rem;
        float: left;
    }
    .productContent .text p{
        clear: both;
        line-height: 0.6rem;
        padding-bottom: 0.3rem;
    }
    .productContent .text p span{
        font-weight: bold;
    }
    .productContent .text p span:first-child{
        color: #386998;
        font-size: 0.35rem;
    }
    .productContent .text p span:nth-child(2){
        color: #db6066;
        font-size: 0.35rem;
    }
    .productContent .text p span:nth-child(3){
        color: #db6066;
        font-size: 0.5rem;
        margin-left: -0.1rem;
    }
    .productContent .text p span:nth-child(4){
        color: #999999;
        font-size: 0.3877rem;
    }




/*    .articleContent{
        margin-bottom: 1.7rem;
    }*/
    .articleLi {
    width: 100%;
    min-height: 2.5rem;
    position: relative;
    overflow: hidden;
    background: white;
    margin-bottom: 0.13rem;
}

    .articleLi img {
        height: 2.12rem;
        width: 3.1rem;
        float: left;
        margin-top: 0.2rem;
    }

    .articleLi .text {
        float: left;
        margin-left: 0.3rem;
        width: 57%;
    }

    .articleLi .text h3 {
        font-size: 0.55rem;
        color: #386998;
        margin-top: 0.2rem;
    }

    .biao {
        background-color: #3998e8;
        color: #fff;
        position: absolute;
        right: -0.7rem;
        top: 0.1rem;
        font-size: 0.4403rem;
        font-weight: 600;
        -webkit-transform: rotate(45deg);
        transform: rotate(45deg);
        padding: 0.1rem 0.7rem;
        text-align: center;
    }

    .time {
        position: absolute;
        bottom: 0.2rem;
        font-size: 0.3rem;
        color: #9e9e9e;
        float: left;
        /*margin-top: 0.4rem;*/
    }

    .custom {
        float: left;
        font-size: 0.4rem;
        color: #386998;
        position: absolute;
        bottom: 0.2rem;
        margin-left: 2.5rem;
    }



.bargain {
    display: block;
    line-height: 1rem;
    font-size: 0.5rem;
    background-color: #ffe400;
    color: #ed535d;
    padding-left: 0.5rem;
    padding-right: 0.5rem;
    font-weight: bold;
    position: absolute;
    /*right: 0;*/
    left: 0;
    bottom: 0;
    height: 1rem;
}
.bargain:after {
    content: "";
    width: 0;
    border: 0.71rem solid #ffe400;
    /*border-right: 1.5rem solid transparent;*/
    border-bottom: none;
    border-left: none;
    position: absolute;
    left: 2.65rem;
    top: 4px;
    transform: rotate(-135deg);
}
.productBargain {
    display: block;
    line-height: 1rem;
    font-size: 0.5rem;
    background-color: #ffe400;
    color: #ed535d;
    padding-left: 0.5rem;
    padding-right: 0.5rem;
    font-weight: bold;
    position: absolute;
    /*right: 0;*/
    left: 0;
    top:0;
    height: 1rem;
}
.productBargain:after {
    content: "";
    width: 0;
    border: 0.71rem solid #ffe400;
    /*border-right: 1.5rem solid transparent;*/
    border-bottom: none;
    border-left: none;
    position: absolute;
    left: 2.65rem;
    top: 4px;
    transform: rotate(-135deg);
}
