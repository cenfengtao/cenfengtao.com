.search-head {

        background: #386998;
    }

    .shop-list .ty-tools-search {
        left: 3vw;
        position: relative;

    }

    .shop-list .ty-tools-search .input-jump {
        background: #fff;
        color: #2f8eba;
    }

    .shop-list .ty-tools-search i::before {
        top: .8rem;
        color: #2f8eba;
    }

    .shop-list .ty-tools-cityswitch {
        top: 3.2rem;
        color: #fff;
    }

    .shop-list .kap-input-value, [data-icon-blue]::before {
        color: #fff;
    }

    .institu-list {
        text-align: center;

    }

    .institu-list li {
        border-top: 10px solid #f0f1f1;
        line-height: 1.2rem;
    }

    .institu-list .shoplogo img {
        margin-top: 0rem;
    }

    .institu-list .shoplogo span {
        font-weight: bold;

    }

    .institu-list .details span {
        width: 4rem;
        border-radius: .111rem;
        text-align: center;
        background-color: #386998;
        color: #fff;
        height: 1.4rem;
        line-height: 1.4rem;
        display: inline-block;
    }

    .institu-list .details span:nth-child(2) {
        margin-left: .5rem;
        background-color: #db6066;
    }

    .institu-list label {
        font-size: .6rem;
        font-weight: 600;
    }

    .institu-list .score {
        line-height: 1.1rem;
        margin-bottom: 1rem
    }

    .institu-list .score span, .institu-list .score i {
        font-size: 0.6rem;
        font-weight: normal;
    }

    .top-nav-v3 .lists-v3 a {
        width: 3rem;
    }

    .empty {
        line-height: 3rem !important;
    }

    .full-star {
        display: inline-block;
        background: url(../../../../Public/images/star-full.png) no-repeat center center;
        width: 0.8rem;
        height: 0.8rem;
        background-size: 100%;
    }
    .empty-star {
        display: inline-block;
        background: url(../../../../Public/images/star-empty.png) no-repeat center center;
        width: 0.8rem;
        height: 0.8rem;
        background-size: 100%;
    }
    .ty-tools-search{
        left: inherit!important;
        margin: auto;
        line-height: 2.4rem;
    }
    .org{
        border: 1px solid;
        height: 1.5rem;
        width: 100%;
        border-radius: 1rem;
        padding: 0.2rem;
        line-height: 1.5rem;
        font-size: 0.7rem;
        font-weight: bold;
    }
    .ty-tools-search i:before{
        top: 0.1rem!important;
    }
/*    .iconfont, [data-icon]:before{
        font-size: 0.8rem!important;
    }*/
    .search-head{
        position: relative;
    }
    .find{
        position: absolute;
        right: 3rem;
        font-size: 1rem;
        color: white;
        top: 0.8rem;
    }